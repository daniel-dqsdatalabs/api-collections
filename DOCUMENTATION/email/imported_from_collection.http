#!/usr/bin/env dothttp

// Send an email

@name("/email/")
POST "https://{{baseUrl}}/email/"
"Content-Type": "application/json"
json({
    "foo": "culpa in"
})


// Send an test email

@name("/email/test")
POST "https://{{baseUrl}}/email/test"
"Content-Type": "application/json"
json({
    "foo": "culpa in"
})


// Get the email settings

@name("/email/settings")
GET "https://{{baseUrl}}/email/settings"



