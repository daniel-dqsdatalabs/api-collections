#!/usr/bin/env dothttp

@name("Get Current Weather")
GET "https://api.openweathermap.org/data/2.5/weather"
? "q"= "{{city}},{{country}}"
? "APPID"= "{{apikey}}"


@name("Get 5 day forecast")
GET "https://api.openweathermap.org/data/2.5/forecast"
? "q"= "{{city}},{{country}}"
? "APPID"= "{{apikey}}"



