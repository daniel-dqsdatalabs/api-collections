#!/usr/bin/env dothttp

@name("By country")
GET "http://api.coronatracker.com/v2/analytics/country"


@name("By new daily cases and deaths")
GET "http://api.coronatracker.com/v3/analytics/dailyNewStats"


@name("Daily new incidences of a country between start and end dates")
GET "http://api.coronatracker.com/v3/analytics/newcases/country"


@name("Data of a country between start and end dates")
GET "http://api.coronatracker.com/v3/analytics/trend/country"



