#!/usr/bin/env dothttp

@name("Get All Network Interfaces")
GET "https://api.pure1.purestorage.com/api/{{api_version}}/network-interfaces"


@name("Get Enabled Network Interfaces w/ sub-interfaces")
GET "https://api.pure1.purestorage.com/api/{{api_version}}/network-interfaces"
? "filter"= "enabled='true' and subinterfaces"



