#!/usr/bin/env dothttp

// Get all available `Markets`. Optional query params **language** or **country** may be used to filter the result.
@name("Get all available markets.")
GET "https://{{baseUrl}}/markets"
? "language"= "de"
? "country"= "DE"
? "fieldsFilter"= "Ut quis"
? "fieldsFilter"= "incididunt elit in enim"



