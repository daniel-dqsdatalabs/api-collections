#!/usr/bin/env dothttp

// Returns a list of makes of cars.
// 
// Website - https://vpic.nhtsa.dot.gov/api/

@name("Makes of Cars")
GET "https://vpic.nhtsa.dot.gov/api/vehicles/getallmakes"
? "format"= "json"


// Returns a list of car manufacturers.
// 
// Website - https://vpic.nhtsa.dot.gov/api/

@name("Car Manufacturers")
GET "https://vpic.nhtsa.dot.gov/api/vehicles/getallmanufacturers"
? "format"= "json"



