#!/usr/bin/env dothttp

// Query the calendar for today.

@name("Today")
GET "http://calapi.inadiutorium.cz/api/v0/en/calendars/general-en/today"



// Query the calendar for tomorrow.

@name("Tomorrow")
GET "http://calapi.inadiutorium.cz/api/v0/en/calendars/general-en/tomorrow"



// Query the calendar for yesterday.

@name("Yesterday")
GET "http://calapi.inadiutorium.cz/api/v0/en/calendars/general-en/yesterday"




