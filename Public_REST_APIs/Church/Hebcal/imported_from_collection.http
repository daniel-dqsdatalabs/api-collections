#!/usr/bin/env dothttp

// Providing Jewish calendar data.

@name("Jewish Calendar")
GET "https://www.hebcal.com/hebcal/"
? "v"= "1"
? "cfg"= "json"
? "maj"= "on"
? "min"= "on"
? "mod"= "on"
? "nx"= "on"
? "year"= "now"
? "month"= "x"
? "ss"= "on"
? "mf"= "on"
? "c"= "on"
? "geo"= "geoname"
? "geonameid"= "3448439"
? "m"= "50"
? "s"= "on"


// Providing Shabbat Times

@name("Shabbat Times")
GET "https://www.hebcal.com/shabbat/"
? "cfg"= "json"
? "geonameid"= "3448439"
? "m"= "50"



