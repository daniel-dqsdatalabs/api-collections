#!/usr/bin/env dothttp

// Like a post by user

@name("http://localhost:9000/posts/1/like")
POST "http://localhost/posts/1/like"
json({
    "userId": 2
})


// Dislike a post by user

@name("http://localhost:9000/posts/1/dislike")
POST "http://localhost/posts/1/dislike"
json({
    "userId": 2
})



