    },
    {
      "type": "SHORT_CODE",
      "capabilities": [
        "SMS"
      ],
      "selected": false,
      "available": true,
      "number": "894545"
    },
    {
      "type": "TOLL_FREE",
      "capabilities": [
        "SMS"
      ],
      "selected": true,
      "available": true,
      "number": "+18544440099"
    },
    {
      "type": "TOLL_FREE",
      "capabilities": [
        "SMS"
      ],
