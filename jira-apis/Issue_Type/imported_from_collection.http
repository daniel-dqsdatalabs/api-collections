#!/usr/bin/env dothttp

# imported from https://www.getpostman.com/collections/d8ed5a08800a7e606296

@name("Get Issue Type Property Keys")
GET "None://{{baseUrl}}/rest/api/3/issuetype/:issueTypeId/properties"


@name("Get All Issue Types For User")
GET "{{baseUrl}}/rest/api/3/issuetype"


@name("Create Issue Type")
POST "{{baseUrl}}/rest/api/3/issuetype"
"Content-Type": "application/json"
data('{
    "name": "<string>",
    "description": "<string>",
    "type": "<string>"
}')


@name("Set Issue Type Property")
PUT "None://{{baseUrl}}/rest/api/3/issuetype/:issueTypeId/properties/:propertyKey"
"Content-Type": "application/json"
data('{}')


@name("Get Issue Type Property")
GET "None://{{baseUrl}}/rest/api/3/issuetype/:issueTypeId/properties/:propertyKey"


@name("Delete Issue Type Property")
DELETE "None://{{baseUrl}}/rest/api/3/issuetype/:issueTypeId/properties/:propertyKey"


@name("Get Issue Type")
GET "None://{{baseUrl}}/rest/api/3/issuetype/:id"


@name("Update Issue Type")
PUT "None://{{baseUrl}}/rest/api/3/issuetype/:id"
"Content-Type": "application/json"
data('{
    "name": "<string>",
    "description": "<string>",
    "avatarId": "<long>"
}')


@name("Delete Issue Type")
DELETE "None://{{baseUrl}}/rest/api/3/issuetype/:id"
? "alternativeIssueTypeId"= "<string>"


@name("Get Alternative Issue Types")
GET "None://{{baseUrl}}/rest/api/3/issuetype/:id/alternatives"


@name("Load Issue Type Avatar")
POST "None://{{baseUrl}}/rest/api/3/issuetype/:id/avatar2"
"Content-Type": "*/*"
? "x"= "0"
? "y"= "0"
? "size"= "<integer>"
data('{}')



