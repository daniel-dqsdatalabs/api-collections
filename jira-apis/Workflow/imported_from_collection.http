#!/usr/bin/env dothttp

# imported from https://www.getpostman.com/collections/d8ed5a08800a7e606296

@name("Get All Workflows")
GET "None://{{baseUrl}}/rest/api/3/workflow"
? "workflowName"= "<string>"


@name("Get Workflows Paginated")
GET "None://{{baseUrl}}/rest/api/3/workflow/search"
? "startAt"= "0"
? "maxResults"= "50"
? "workflowName"= "<string>"
? "expand"= "<string>"



