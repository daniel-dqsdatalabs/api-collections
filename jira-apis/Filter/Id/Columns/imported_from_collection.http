#!/usr/bin/env dothttp

# imported from https://www.getpostman.com/collections/d8ed5a08800a7e606296

@name("Get Columns")
GET "None://{{baseUrl}}/rest/api/3/filter/:id/columns"


@name("Set Columns")
PUT "None://{{baseUrl}}/rest/api/3/filter/:id/columns"
"Content-Type": "multipart/form-data"
files(
	("0", "<string>"),
	("1", "<string>")
)


@name("Reset Columns")
DELETE "None://{{baseUrl}}/rest/api/3/filter/:id/columns"



