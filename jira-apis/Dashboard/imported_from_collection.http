#!/usr/bin/env dothttp

# imported from https://www.getpostman.com/collections/d8ed5a08800a7e606296

@name("Get All Dashboards")
GET "None://{{baseUrl}}/rest/api/3/dashboard"
? "filter"= "<string>"
? "startAt"= "0"
? "maxResults"= "20"


@name("Create Dashboard")
POST "{{baseUrl}}/rest/api/3/dashboard"
"Content-Type": "application/json"
data('{
    "name": "<string>",
    "sharePermissions": [
        {
            "type": "<string>",
            "project": {
                "id": "<string>",
                "email": "<string>",
                "favourite": "<boolean>"
            },
            "role": {
                "name": "<string>",
                "translatedName": "<string>",
                "currentUserRole": "<boolean>"
            },
            "group": {
                "name": "<string>"
            }
        },
        {
            "type": "<string>",
            "project": {
                "id": "<string>",
                "email": "<string>",
                "favourite": "<boolean>"
            },
            "role": {
