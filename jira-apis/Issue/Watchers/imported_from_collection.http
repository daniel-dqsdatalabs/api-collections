#!/usr/bin/env dothttp

# imported from https://www.getpostman.com/collections/d8ed5a08800a7e606296

@name("Get Issue Watchers")
GET "None://{{baseUrl}}/rest/api/3/issue/:issueIdOrKey/watchers"


@name("Add Watcher")
POST "None://{{baseUrl}}/rest/api/3/issue/:issueIdOrKey/watchers"
"Content-Type": "application/json"
data('"<string>"')


@name("Delete Watcher")
DELETE "None://{{baseUrl}}/rest/api/3/issue/:issueIdOrKey/watchers"
? "username"= "<string>"
? "accountId"= "<string>"



