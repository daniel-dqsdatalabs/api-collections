    "properties": [
        {
            "key": "<string>",
            "value": {}
        },
        {
            "key": "<string>",
            "value": {}
        }
    ]
}')


@name("Delete Comment")
DELETE "None://{{baseUrl}}/rest/api/3/issue/:issueIdOrKey/comment/:id"



