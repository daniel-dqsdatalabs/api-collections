#!/usr/bin/env dothttp

# imported from https://www.getpostman.com/collections/d8ed5a08800a7e606296

@name("Get Comments By Ids")
POST "None://{{baseUrl}}/rest/api/3/comment/list"
"Content-Type": "application/json"
? "expand"= "<string>"
data('{
    "ids": [
        "<long>"
    ]
}')


@name("Get Comment Property")
GET "None://{{baseUrl}}/rest/api/3/comment/:commentId/properties/:propertyKey"


@name("Set Comment Property")
PUT "None://{{baseUrl}}/rest/api/3/comment/:commentId/properties/:propertyKey"
"Content-Type": "application/json"
data('{}')


@name("Delete Comment Property")
DELETE "None://{{baseUrl}}/rest/api/3/comment/:commentId/properties/:propertyKey"


@name("Get Comment Property Keys")
GET "None://{{baseUrl}}/rest/api/3/comment/:commentId/properties"



