#!/usr/bin/env dothttp

# imported from https://www.getpostman.com/collections/d8ed5a08800a7e606296

@name("Get Project Role By Id")
GET "None://{{baseUrl}}/rest/api/3/role/:id"


@name("Fully Update Project Role")
PUT "None://{{baseUrl}}/rest/api/3/role/:id"
"Content-Type": "application/json"
data('{
    "name": "<string>",
    "description": "<string>"
}')


@name("Partial Update Project Role")
POST "None://{{baseUrl}}/rest/api/3/role/:id"
"Content-Type": "application/json"
data('{
    "name": "<string>",
    "description": "<string>"
}')


@name("Delete Project Role")
DELETE "None://{{baseUrl}}/rest/api/3/role/:id"
? "swap"= "<long>"



