#!/usr/bin/env dothttp

# imported from https://www.getpostman.com/collections/d8ed5a08800a7e606296

@name("Get Default Actors For Project Role")
GET "None://{{baseUrl}}/rest/api/3/role/:id/actors"


@name("Add Default Actors To Project Role")
POST "None://{{baseUrl}}/rest/api/3/role/:id/actors"
"Content-Type": "application/json"
data('{
    "user": [
        "<string>",
        "<string>"
    ],
    "group": [
        "<string>",
        "<string>"
    ]
}')


@name("Delete Default Actors From Project Role")
DELETE "None://{{baseUrl}}/rest/api/3/role/:id/actors"
? "user"= "<string>"
? "group"= "<string>"



