#!/usr/bin/env dothttp

# imported from https://www.getpostman.com/collections/d8ed5a08800a7e606296

@name("Get User Property")
GET "None://{{baseUrl}}/rest/api/3/user/properties/:propertyKey"
? "accountId"= "<string>"
? "userKey"= "<string>"
? "username"= "<string>"


@name("Set User Property")
PUT "None://{{baseUrl}}/rest/api/3/user/properties/:propertyKey"
"Content-Type": "application/json"
? "accountId"= "<string>"
? "userKey"= "<string>"
? "username"= "<string>"
data('{}')


@name("Delete User Property")
DELETE "None://{{baseUrl}}/rest/api/3/user/properties/:propertyKey"
? "accountId"= "<string>"
? "userKey"= "<string>"
? "username"= "<string>"



