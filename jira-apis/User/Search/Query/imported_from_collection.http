#!/usr/bin/env dothttp

# imported from https://www.getpostman.com/collections/d8ed5a08800a7e606296

@name("Find Users By Query")
GET "None://{{baseUrl}}/rest/api/3/user/search/query"
? "query"= "<string>"
? "startAt"= "0"
? "maxResults"= "100"


@name("Find User Keys By Query")
GET "None://{{baseUrl}}/rest/api/3/user/search/query/key"
? "query"= "<string>"
? "startAt"= "0"
? "maxResults"= "100"



