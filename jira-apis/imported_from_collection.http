#!/usr/bin/env dothttp

# imported from https://www.getpostman.com/collections/d8ed5a08800a7e606296

@name("Get Jira Instance Info")
GET "{{baseUrl}}/rest/api/3/serverInfo"


@name("Get Issue Security Level")
GET "None://{{baseUrl}}/rest/api/3/securitylevel/:id"


@name("Get Current User")
GET "None://{{baseUrl}}/rest/api/3/myself"
basicauth("{{atlassian_email}}", "{{atlassian_token}}")
? "expand"= "<string>"


@name("Get My Permissions")
GET "None://{{baseUrl}}/rest/api/3/mypermissions"
? "projectKey"= "<string>"
? "projectId"= "<string>"
? "issueKey"= "<string>"
? "issueId"= "<string>"
? "permissions"= "<string>"
? "projectUuid"= "<string>"
? "projectConfigurationUuid"= "<string>"


@name("Get Custom Field Option")
GET "None://{{baseUrl}}/rest/api/3/customFieldOption/:id"


@name("Get Audit Records")
GET "None://{{baseUrl}}/rest/api/3/auditing/record"
? "offset"= "0"
? "limit"= "1000"
? "filter"= "<string>"
? "from"= "<dateTime>"
? "to"= "<dateTime>"


@name("Get System Avatars By Type")
GET "None://{{baseUrl}}/rest/api/3/avatar/:type/system"


@name("Find Groups")
GET "None://{{baseUrl}}/rest/api/3/groups/picker"
? "accountId"= "<string>"
? "query"= "<string>"
? "exclude"= "<string>"
? "exclude"= "<string>"
? "maxResults"= "<integer>"
? "userName"= "<string>"


@name("Find Users And Groups")
GET "None://{{baseUrl}}/rest/api/3/groupuserpicker"
? "query"= "<string>"
? "maxResults"= "50"
? "showAvatar"= "false"
? "fieldId"= "<string>"
? "projectId"= "<string>"
? "projectId"= "<string>"
? "issueTypeId"= "<string>"
? "issueTypeId"= "<string>"
? "avatarSize"= "xsmall"
? "caseInsensitive"= "false"
? "excludeConnectAddons"= "false"


@name("Get All Labels")
GET "None://{{baseUrl}}/rest/api/3/label"
basicauth("{{atlassian_email}}", "{{atlassian_token}}")
? "startAt"= "0"
? "maxResults"= "1000"



