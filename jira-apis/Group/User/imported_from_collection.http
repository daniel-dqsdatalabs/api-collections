#!/usr/bin/env dothttp

# imported from https://www.getpostman.com/collections/d8ed5a08800a7e606296

@name("Add User To Group")
POST "None://{{baseUrl}}/rest/api/3/group/user"
"Content-Type": "application/json"
? "groupname"= "<string>"
data('{
    "name": "<string>",
    "accountId": "<string>"
}')


@name("Remove User From Group")
DELETE "None://{{baseUrl}}/rest/api/3/group/user"
? "groupname"= "<string>"
? "username"= "<string>"
? "accountId"= "<string>"



