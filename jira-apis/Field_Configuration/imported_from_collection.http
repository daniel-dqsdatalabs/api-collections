#!/usr/bin/env dothttp

# imported from https://www.getpostman.com/collections/d8ed5a08800a7e606296

@name("Get All Field Configurations")
GET "None://{{baseUrl}}/rest/api/3/fieldconfiguration"
? "startAt"= "0"
? "maxResults"= "50"
? "id"= "<long>"
? "isDefault"= "false"


@name("Get Field Configuration Items")
GET "None://{{baseUrl}}/rest/api/3/fieldconfiguration/:id/fields"
? "startAt"= "0"
? "maxResults"= "50"



