#!/usr/bin/env dothttp

# imported from https://www.getpostman.com/collections/d8ed5a08800a7e606296

@name("Get Version")
GET "None://{{baseUrl}}/rest/api/3/version/:id"
? "expand"= "<string>"


@name("Update Version")
PUT "None://{{baseUrl}}/rest/api/3/version/:id"
"Content-Type": "application/json"
data('{
    "expand": "<string>",
    "description": "<string>",
    "name": "<string>",
    "archived": "<boolean>",
    "released": "<boolean>",
    "startDate": "<date>",
    "releaseDate": "<date>",
    "project": "<string>",
    "projectId": "<long>",
    "moveUnfixedIssuesTo": "<uri>"
}')


@name("Delete Version")
DELETE "None://{{baseUrl}}/rest/api/3/version/:id"
? "moveFixIssuesTo"= "<string>"
? "moveAffectedIssuesTo"= "<string>"


@name("Merge Versions")
PUT "None://{{baseUrl}}/rest/api/3/version/:id/mergeto/:moveIssuesTo"


@name("Move Version")
POST "None://{{baseUrl}}/rest/api/3/version/:id/move"
"Content-Type": "application/json"
data('{
    "after": "<uri>",
    "position": "<string>"
}')


@name("Get Version's Related Issues Count")
GET "None://{{baseUrl}}/rest/api/3/version/:id/relatedIssueCounts"


@name("Delete And Replace Version")
POST "None://{{baseUrl}}/rest/api/3/version/:id/removeAndSwap"
"Content-Type": "application/json"
data('{
    "moveFixIssuesTo": "<long>",
    "moveAffectedIssuesTo": "<long>",
    "customFieldReplacementList": [
        {
            "customFieldId": "<long>",
            "moveTo": "<long>"
        },
        {
            "customFieldId": "<long>",
            "moveTo": "<long>"
        }
    ]
}')


@name("Get Version's Unresolved Issues Count")
GET "None://{{baseUrl}}/rest/api/3/version/:id/unresolvedIssueCount"



