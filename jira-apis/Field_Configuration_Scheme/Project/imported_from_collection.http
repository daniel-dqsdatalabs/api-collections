#!/usr/bin/env dothttp

# imported from https://www.getpostman.com/collections/d8ed5a08800a7e606296

@name("Get Field Configuration Schemes For Projects")
GET "None://{{baseUrl}}/rest/api/3/fieldconfigurationscheme/project"
? "startAt"= "0"
? "maxResults"= "50"
? "projectId"= "<long>"


@name("Assign Field Configuration Scheme To Project")
PUT "{{baseUrl}}/rest/api/3/fieldconfigurationscheme/project"
"Content-Type": "application/json"
data('{
    "projectId": "<string>",
    "fieldConfigurationSchemeId": "<string>"
}')



