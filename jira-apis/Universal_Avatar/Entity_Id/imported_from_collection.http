#!/usr/bin/env dothttp

# imported from https://www.getpostman.com/collections/d8ed5a08800a7e606296

@name("Get Avatars")
GET "None://{{baseUrl}}/rest/api/3/universal_avatar/type/:type/owner/:entityId"


@name("Load Avatar")
POST "None://{{baseUrl}}/rest/api/3/universal_avatar/type/:type/owner/:entityId"
"Content-Type": "*/*"
? "x"= "0"
? "y"= "0"
? "size"= "<integer>"
data('{}')



