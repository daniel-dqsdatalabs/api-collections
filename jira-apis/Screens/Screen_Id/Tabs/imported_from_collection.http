#!/usr/bin/env dothttp

# imported from https://www.getpostman.com/collections/d8ed5a08800a7e606296

@name("Get All Screen Tabs")
GET "None://{{baseUrl}}/rest/api/3/screens/:screenId/tabs"
? "projectKey"= "<string>"


@name("Create Screen Tab")
POST "None://{{baseUrl}}/rest/api/3/screens/:screenId/tabs"
"Content-Type": "application/json"
data('{
    "name": "<string>"
}')



