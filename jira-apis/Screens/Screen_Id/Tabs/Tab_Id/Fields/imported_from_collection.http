#!/usr/bin/env dothttp

# imported from https://www.getpostman.com/collections/d8ed5a08800a7e606296

@name("Get All Screen Tab Fields")
GET "None://{{baseUrl}}/rest/api/3/screens/:screenId/tabs/:tabId/fields"
? "projectKey"= "<string>"


@name("Add Screen Tab Field")
POST "None://{{baseUrl}}/rest/api/3/screens/:screenId/tabs/:tabId/fields"
"Content-Type": "application/json"
data('{
    "fieldId": "<string>"
}')



