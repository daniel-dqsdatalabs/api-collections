#!/usr/bin/env dothttp

# imported from https://www.getpostman.com/collections/d8ed5a08800a7e606296

@name("Get Screens")
GET "None://{{baseUrl}}/rest/api/3/screens"
? "startAt"= "0"
? "maxResults"= "100"
? "id"= "<long>"


@name("Create Screen")
POST "{{baseUrl}}/rest/api/3/screens"
"Content-Type": "application/json"
data('{
    "name": "<string>",
    "description": "<string>"
}')


@name("Add Field To Default Screen")
POST "None://{{baseUrl}}/rest/api/3/screens/addToDefault/:fieldId"



