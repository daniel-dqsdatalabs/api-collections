#!/usr/bin/env dothttp

# imported from https://www.getpostman.com/collections/d8ed5a08800a7e606296

@name("Get Screen Schemes")
GET "None://{{baseUrl}}/rest/api/3/screenscheme"
? "startAt"= "0"
? "maxResults"= "25"
? "id"= "<long>"


@name("Create Screen Scheme")
POST "{{baseUrl}}/rest/api/3/screenscheme"
"Content-Type": "application/json"
data('{
    "name": "<string>",
    "screens": {
        "type": "integer",
        "description": "The ID of the default screen. Required when creating a screen scheme.",
        "default": "<long>"
    },
    "description": "<string>"
}')



