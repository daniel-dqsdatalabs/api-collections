#!/usr/bin/env dothttp

# imported from https://www.getpostman.com/collections/d8ed5a08800a7e606296

@name("Update Screen Scheme")
PUT "None://{{baseUrl}}/rest/api/3/screenscheme/:screenSchemeId"
"Content-Type": "application/json"
data('{
    "name": "<string>",
    "description": "<string>",
    "screens": {
        "type": "string",
        "description": "The ID of the default screen. When specified, must include a screen ID as a default screen is required.",
        "writeOnly": true,
        "default": "<string>"
    }
}')


@name("Delete Screen Scheme")
DELETE "None://{{baseUrl}}/rest/api/3/screenscheme/:screenSchemeId"



