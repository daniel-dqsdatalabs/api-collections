#!/usr/bin/env dothttp

# imported from https://www.getpostman.com/collections/d8ed5a08800a7e606296

@name("Get Issue Type Schemes For Projects")
GET "None://{{baseUrl}}/rest/api/3/issuetypescheme/project"
? "startAt"= "0"
? "maxResults"= "50"
? "projectId"= "<long>"


@name("Assign Issue Type Scheme To Project")
PUT "{{baseUrl}}/rest/api/3/issuetypescheme/project"
"Content-Type": "application/json"
data('{
    "issueTypeSchemeId": "<string>",
    "projectId": "<string>"
}')



