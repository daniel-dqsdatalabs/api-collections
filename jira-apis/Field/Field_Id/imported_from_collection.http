@name("Get Contexts For A Field")
GET "https://{{baseUrl}}/rest/api/3/field/:fieldId/contexts"
? "startAt"= "0"
? "maxResults"= "20"


@name("Get Screens For A Field")
GET "https://{{baseUrl}}/rest/api/3/field/:fieldId/screens"
? "startAt"= "0"
? "maxResults"= "100"



