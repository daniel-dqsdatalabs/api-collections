#!/usr/bin/env dothttp

# imported from https://www.getpostman.com/collections/d8ed5a08800a7e606296

@name("Update Custom Field Context")
PUT "None://{{baseUrl}}/rest/api/3/field/:fieldId/context/:contextId"
"Content-Type": "application/json"
data('{
    "name": "<string>",
    "description": "<string>"
}')


@name("Delete Custom Field Context")
DELETE "None://{{baseUrl}}/rest/api/3/field/:fieldId/context/:contextId"


@name("Add Issue Types To Context")
PUT "None://{{baseUrl}}/rest/api/3/field/:fieldId/context/:contextId/issuetype"
"Content-Type": "application/json"
data('{
    "issueTypeIds": [
        "<string>",
        "<string>"
    ]
}')



