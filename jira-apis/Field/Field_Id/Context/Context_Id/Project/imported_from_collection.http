#!/usr/bin/env dothttp

# imported from https://www.getpostman.com/collections/d8ed5a08800a7e606296

@name("Assign Custom Field Context To Projects")
PUT "None://{{baseUrl}}/rest/api/3/field/:fieldId/context/:contextId/project"
"Content-Type": "application/json"
data('{
    "projectIds": [
        "<string>",
        "<string>"
    ]
}')


@name("Remove Custom Field Context From Projects")
POST "None://{{baseUrl}}/rest/api/3/field/:fieldId/context/:contextId/project/remove"
"Content-Type": "application/json"
data('{
    "projectIds": [
        "<string>",
        "<string>"
    ]
}')



