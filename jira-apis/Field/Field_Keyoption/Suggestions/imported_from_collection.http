#!/usr/bin/env dothttp

# imported from https://www.getpostman.com/collections/d8ed5a08800a7e606296

@name("Get Selectable Issue Field Options")
GET "None://{{baseUrl}}/rest/api/3/field/:fieldKey/option/suggestions/edit"
? "startAt"= "0"
? "maxResults"= "50"
? "projectId"= "<long>"


@name("Get Visible Issue Field Options")
GET "None://{{baseUrl}}/rest/api/3/field/:fieldKey/option/suggestions/search"
? "startAt"= "0"
? "maxResults"= "<integer>"
? "projectId"= "<long>"



