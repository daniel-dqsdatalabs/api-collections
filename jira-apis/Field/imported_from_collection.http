#!/usr/bin/env dothttp

# imported from https://www.getpostman.com/collections/d8ed5a08800a7e606296

@name("Get Fields")
GET "{{baseUrl}}/rest/api/3/field"


@name("Create Custom Field")
POST "{{baseUrl}}/rest/api/3/field"
"Content-Type": "application/json"
data('{
    "name": "<string>",
    "searcherKey": "<string>",
    "type": "<string>",
    "description": "<string>"
}')


@name("Get Fields Paginated")
GET "None://{{baseUrl}}/rest/api/3/field/search"
? "startAt"= "0"
? "maxResults"= "50"
? "type"= "<string>"
? "type"= "<string>"
? "id"= "<string>"
? "query"= "<string>"
? "orderBy"= "<string>"
? "expand"= "<string>"



