#!/usr/bin/env dothttp

# imported from https://www.getpostman.com/collections/d8ed5a08800a7e606296

@name("Get Project Role For Project")
GET "None://{{baseUrl}}/rest/api/3/project/:projectIdOrKey/role/:id"


@name("Set Actors For Project Role")
PUT "None://{{baseUrl}}/rest/api/3/project/:projectIdOrKey/role/:id"
"Content-Type": "application/json"
data('{
    "categorisedActors": "<object>"
}')


@name("Add Actors To Project Role")
POST "None://{{baseUrl}}/rest/api/3/project/:projectIdOrKey/role/:id"
"Content-Type": "application/json"
data('{
    "user": [
        "<string>",
        "<string>"
    ],
    "group": [
        "<string>",
        "<string>"
    ]
}')


@name("Delete Actors From Project Role")
DELETE "None://{{baseUrl}}/rest/api/3/project/:projectIdOrKey/role/:id"
? "user"= "<string>"
? "group"= "<string>"



