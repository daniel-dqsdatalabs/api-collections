#!/usr/bin/env dothttp

# imported from https://www.getpostman.com/collections/d8ed5a08800a7e606296

@name("Get Assigned Permission Scheme")
GET "None://{{baseUrl}}/rest/api/3/project/:projectKeyOrId/permissionscheme"
? "expand"= "<string>"


@name("Assign Permission Scheme")
PUT "None://{{baseUrl}}/rest/api/3/project/:projectKeyOrId/permissionscheme"
"Content-Type": "application/json"
? "expand"= "<string>"
data('{
    "id": "<long>"
}')



