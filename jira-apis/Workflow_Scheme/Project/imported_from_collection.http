#!/usr/bin/env dothttp

# imported from https://www.getpostman.com/collections/d8ed5a08800a7e606296

@name("Get Workflow Scheme Project Associations")
GET "None://{{baseUrl}}/rest/api/3/workflowscheme/project"
? "projectId"= "<long>"


@name("Assign Workflow Scheme To Project")
PUT "{{baseUrl}}/rest/api/3/workflowscheme/project"
"Content-Type": "application/json"
data('{
    "projectId": "<string>",
    "workflowSchemeId": "<string>"
}')



