#!/usr/bin/env dothttp

# imported from https://www.getpostman.com/collections/d8ed5a08800a7e606296

@name("Get Workflow For Issue Type In Workflow Scheme")
GET "None://{{baseUrl}}/rest/api/3/workflowscheme/:id/issuetype/:issueType"
? "returnDraftIfExists"= "false"


@name("Set Workflow For Issue Type In Workflow Scheme")
PUT "None://{{baseUrl}}/rest/api/3/workflowscheme/:id/issuetype/:issueType"
"Content-Type": "application/json"
data('{
    "issueType": "<string>",
    "workflow": "<string>",
    "updateDraftIfNeeded": "<boolean>"
}')


@name("Delete Workflow For Issue Type In Workflow Scheme")
DELETE "None://{{baseUrl}}/rest/api/3/workflowscheme/:id/issuetype/:issueType"
? "updateDraftIfNeeded"= "<boolean>"



