#!/usr/bin/env dothttp

# imported from https://www.getpostman.com/collections/d8ed5a08800a7e606296

@name("Get Default Workflow")
GET "None://{{baseUrl}}/rest/api/3/workflowscheme/:id/default"
? "returnDraftIfExists"= "false"


@name("Update Default Workflow")
PUT "None://{{baseUrl}}/rest/api/3/workflowscheme/:id/default"
"Content-Type": "application/json"
data('{
    "workflow": "<string>",
    "updateDraftIfNeeded": "<boolean>"
}')


@name("Delete Default Workflow")
DELETE "None://{{baseUrl}}/rest/api/3/workflowscheme/:id/default"
? "updateDraftIfNeeded"= "<boolean>"



