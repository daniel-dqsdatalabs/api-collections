#!/usr/bin/env dothttp

# imported from https://www.getpostman.com/collections/d8ed5a08800a7e606296

@name("Get Permission Scheme Grants")
GET "None://{{baseUrl}}/rest/api/3/permissionscheme/:schemeId/permission"
? "expand"= "<string>"


@name("Create Permission Grant")
POST "None://{{baseUrl}}/rest/api/3/permissionscheme/:schemeId/permission"
"Content-Type": "application/json"
? "expand"= "<string>"
data('{
    "holder": {
        "type": "<string>",
        "parameter": "<string>"
    },
    "permission": "<string>"
}')



