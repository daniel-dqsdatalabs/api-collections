#!/usr/bin/env dothttp

// This endpoint requires the season(year) and race(round) to be specified.

@name("Pit stop data for a race ")
GET "http://ergast.com/api/f1/{{year}}/{{round}}/pitstops"


// This endpoint requires the season(year), race(round) and pit stop number(pitstopnumber) to be specified.

@name("Information for a specific pit stop")
GET "http://ergast.com/api/f1/{{year}}/{{round}}/pitstops/{{pitstopnumber}}"



