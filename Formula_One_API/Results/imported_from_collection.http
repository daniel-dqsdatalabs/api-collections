#!/usr/bin/env dothttp

// This endpoint gives the result for a specific race (round) in a season(year).// // // If the results for the specified race are not yet available the RaceTable element in the response will be empty.// 
@name("Race Result")
GET "http://ergast.com/api/f1/{{year}}/{{round}}/results"


// Results from the most recent race can be obtained using this endpoint.
@name("Most recent race result")
GET "http://ergast.com/api/f1/current/last/results"



