#!/usr/bin/env dothttp

// Get the List of Expertises currently available for users to choose from. 

@name("Get List of Expertises")
GET "https://{{URL}}/getExpertiseList/"
"Authorization": "Token {{token}}"


// Get the List of Expertises by USER ID

@name("Get the Expertises that belong to a User")
GET "https://{{URL}}/getExpertise/{{user_id}}"
"Authorization": "Token {{token}}"



