#!/usr/bin/env dothttp

// Request a Login Authetication token for a user.

@name("Login to the System")
POST "https://{{URL}}/rest-auth/login/"
"Content-Type": "application/x-www-form-urlencoded"
data({
    "username": [
        "{{username}}"
    ],
    "password": [
        "{{password}}"
    ]
})



