#!/usr/bin/env dothttp

@name("Command Custom WallPaper Upload (Awake)")
POST "https://{{endpoint}}/putxml"
"Content-Type": "text/xml"
"Authorization": "Basic {{credentials}}"
data('<Command>
 <UserInterface>
 	<Branding>
 		<Upload>
 			<Type>Background</Type>
 			<body></body>
 		</Upload>
 	</Branding>
 </UserInterface>
</Command>')


@name("Configuration Wallpaper (Auto)")
POST "https://{{endpoint}}/putxml"
"Content-Type": "text/xml"
"Authorization": "Basic {{credentials}}"
data('<Configuration>
 <UserInterface>
 	<Wallpaper>Auto</Wallpaper>
 </UserInterface>
</Configuration>')


@name("Configuration Wallpaper (Custom)")
POST "https://{{endpoint}}/putxml"
"Content-Type": "text/xml"
"Authorization": "Basic {{credentials}}"
data('<Configuration>
 <UserInterface>
 	<Wallpaper>Custom</Wallpaper>
 </UserInterface>
</Configuration>')


@name("Command Custom WallPaper Delete")
POST "https://{{endpoint}}/putxml"
"Content-Type": "text/xml"
"Authorization": "Basic {{credentials}}"
data('<Command>
 <UserInterface>
 	<Branding>
 		<Delete>
 			<Type>Background</Type>
 		</Delete>
 	</Branding>
 </UserInterface>
</Command>')



