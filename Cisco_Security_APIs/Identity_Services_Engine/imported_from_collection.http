#!/usr/bin/env dothttp

// Get all ANC policies from ISE

@name("All ANC Policies")
GET "https://{{ise_host}}/ers/config/ancpolicy"
"Content-Type": "application/json"
"Accept": "application/json"


// Get all ANC Endpoints from ISE

@name("All ANC Endpoints")
GET "https://{{ise_host}}/ers/config/ancendpoint"
"Content-Type": "application/json"
"Accept": "application/json"



