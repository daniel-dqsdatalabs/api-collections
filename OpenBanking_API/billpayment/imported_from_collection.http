#!/usr/bin/env dothttp

@name("getBillPaymentItem")
GET "http://api.openbanking.ng/billpayment/getBillPaymentItem"
"Accept": "*/*"
"Content-Type": "application/json"
data('Biller ID ')


@name("getBillerCategories")
