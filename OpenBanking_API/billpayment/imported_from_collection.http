@name("getBillers")
GET "http://api.openbanking.ng/billpayment/getBillers"
"Accept": "*/*"
"Content-Type": "application/json"
data('Category ID ')


@name("getBillingSystem")
GET "http://api.openbanking.ng/billpayment/getBillingSystem"
"Accept": "*/*"
"Content-Type": "application/json"




