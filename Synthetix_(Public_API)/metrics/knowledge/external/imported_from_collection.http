#!/usr/bin/env dothttp

// Gets a summary of recorded activity against a specific article.  Including number of hits from FAQ lists or from search results, feedback, comments.

@name("article_history")
GET "https://{{HOST}}/{{BASE}}/metrics/knowledge/external/article_history"
"APPLICATIONKEY": "{{APPLICATIONKEY}}"
"CONSUMERKEY": "{{CONSUMERKEY}}"
? "appkey"= "5779133dfc44d74723c40ebdd73e9e8f"
? "startdate"= "2018-06-21"
? "enddate"= "2018-06-27"
? "search"= "lbl00019"


// Returns a summary of the top articles by category and subcategory by date for the period between the start date and end date.  If no start date and end date are passed, returns data for 'today'.  For each article, returns questions, label, trigger, hits from FAQ lists and hits from search results.  Articles are also grouped by trigger.

@name("categories")
GET "https://{{HOST}}/{{BASE}}/metrics/knowledge/external/categories"
"APPLICATIONKEY": "{{APPLICATIONKEY}}"
"CONSUMERKEY": "{{CONSUMERKEY}}"
? "startdate"= "2018-02-06"
? "enddate"= "2018-03-07"
? "channel"= "14"
? "view"= "3159"
? "internal"= "true"
? "appkey"= "5779133dfc44d74723c40ebdd73e9e8f"
? "limit"= "10"


// Returns a count of the number of interactions for each recorded useragent over the selected period.

@name("device")
GET "https://{{HOST}}/{{BASE}}/metrics/knowledge/external/device"
"APPLICATIONKEY": "{{APPLICATIONKEY}}"
"CONSUMERKEY": "{{CONSUMERKEY}}"
? "appkey"= "5779133dfc44d74723c40ebdd73e9e8f"
? "startdate"= "2018-03-01"
? "enddate"= "2018-03-08"


@name("feedback")
GET "https://{{HOST}}/{{BASE}}/metrics/knowledge/external/feedback"
"APPLICATIONKEY": "{{APPLICATIONKEY}}"
"CONSUMERKEY": "{{CONSUMERKEY}}"
? "appkey"= "5779133dfc44d74723c40ebdd73e9e8f"
? "startdate"= "2018-02-07"
? "enddate"= "2018-02-09"
? "label"= "lbl00017"


@name("escalations")
GET "https://{{HOST}}/{{BASE}}/metrics/knowledge/external/escalations"
"APPLICATIONKEY": "{{APPLICATIONKEY}}"
"CONSUMERKEY": "{{CONSUMERKEY}}"
? "appkey"= "5779133dfc44d74723c40ebdd73e9e8f"
? "startdate"= "2017-12-22"
? "enddate"= "2018-02-11"
? "view"= "FAQs"


// Returns a summary by date and view of the number of sessions generated by city.

@name("location")
GET "https://{{HOST}}/{{BASE}}/metrics/knowledge/external/location"
"APPLICATIONKEY": "{{APPLICATIONKEY}}"
"CONSUMERKEY": "{{CONSUMERKEY}}"
? "appkey"= "5779133dfc44d74723c40ebdd73e9e8f"
? "startdate"= "2017-12-22"
? "enddate"= "2018-02-11"
? "view"= "FAQs"


// Returns a summary by date and view of the triggers offered and accepted for a given application.

@name("trigger")
GET "https://{{HOST}}/{{BASE}}/metrics/knowledge/external/trigger"
"APPLICATIONKEY": "{{APPLICATIONKEY}}"
"CONSUMERKEY": "{{CONSUMERKEY}}"
? "startdate"= "2018-02-06"
? "enddate"= "2018-03-07"
? "channel"= "14"
? "view"= "3159"
? "internal"= "true"
? "appkey"= "5779133dfc44d74723c40ebdd73e9e8f"



