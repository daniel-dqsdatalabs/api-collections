#!/usr/bin/env dothttp

@name("List Country Specs")
GET "https://{{baseUrl}}/v1/country_specs"
"Content-Type": "application/x-www-form-urlencoded"
? "ending_before"= "<string>"
? "expand[0]"= "<string>"
? "expand[1]"= "<string>"
? "limit"= "<integer>"
? "starting_after"= "<string>"


@name("Retrieve a Country Spec")
GET "https://{{baseUrl}}/v1/country_specs/:country"
"Content-Type": "application/x-www-form-urlencoded"
? "expand[0]"= "<string>"
? "expand[1]"= "<string>"



