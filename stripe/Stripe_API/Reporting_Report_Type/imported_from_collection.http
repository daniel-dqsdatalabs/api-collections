#!/usr/bin/env dothttp

@name("List all report types")
GET "https://{{baseUrl}}/v1/reporting/report_types"
"Content-Type": "application/x-www-form-urlencoded"
? "expand[0]"= "<string>"
? "expand[1]"= "<string>"


@name("Retrieve a report type")
GET "https://{{baseUrl}}/v1/reporting/report_types/:report_type"
"Content-Type": "application/x-www-form-urlencoded"
? "expand[0]"= "<string>"
? "expand[1]"= "<string>"



