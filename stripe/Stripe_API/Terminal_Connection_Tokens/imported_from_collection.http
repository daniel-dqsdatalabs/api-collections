#!/usr/bin/env dothttp

@name("Create a connection token")
POST "https://{{baseUrl}}/v1/terminal/connection_tokens"
"Content-Type": "application/x-www-form-urlencoded"
data({
    "expand[0]": [
        "<string>"
    ],
    "expand[1]": [
        "<string>"
    ],
    "location": [
        "<string>"
    ]
})



