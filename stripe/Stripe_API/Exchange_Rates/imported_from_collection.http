#!/usr/bin/env dothttp

@name("List all exchange rates")
GET "https://{{baseUrl}}/v1/exchange_rates"
"Content-Type": "application/x-www-form-urlencoded"
? "ending_before"= "<string>"
? "expand[0]"= "<string>"
? "expand[1]"= "<string>"
? "limit"= "<integer>"
? "starting_after"= "<string>"


@name("Retrieve an exchange rate")
GET "https://{{baseUrl}}/v1/exchange_rates/:rate_id"
"Content-Type": "application/x-www-form-urlencoded"
? "expand[0]"= "<string>"
? "expand[1]"= "<string>"



