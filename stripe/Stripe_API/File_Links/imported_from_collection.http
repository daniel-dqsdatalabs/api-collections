#!/usr/bin/env dothttp

# imported from http://localhost:8000/Stripe%20API.postman_collection.json

@name("List all file links")
GET "<dotextensions.server.postman2_1.URLClass object at 0x7fd05fc08940>"
"Content-Type": "application/x-www-form-urlencoded"


@name("Create a file link")
POST "<dotextensions.server.postman2_1.URLClass object at 0x7fd05fc094c0>"
"Content-Type": "application/x-www-form-urlencoded"
data({
    "file": [
        "<string>"
    ],
    "expand[0]": [
        "<string>"
    ],
    "expand[1]": [
        "<string>"
    ],
    "expires_at": [
        "<unix-time>"
    ]
})


@name("Retrieve a file link")
GET "<dotextensions.server.postman2_1.URLClass object at 0x7fd05fc09c10>"
"Content-Type": "application/x-www-form-urlencoded"


@name("Update a file link")
POST "<dotextensions.server.postman2_1.URLClass object at 0x7fd05fc0ca60>"
"Content-Type": "application/x-www-form-urlencoded"
data({
    "expand[0]": [
        "<string>"
    ],
    "expand[1]": [
        "<string>"
    ]
})



