#!/usr/bin/env dothttp

@name("Query Entry Requirements")
GET "https://{{baseUrl}}/v2/entry-requirements"
? "citizenship"= "CL"
? "portOfEntry"= "HAN"
? "language"= "es"
? "key"= "{{apiKey}}"
? "affiliateId"= "sherpa"
? "destination"= "JP"


@name("Swagger.json")
GET "https://{{baseUrl}}/swagger.json"
? "key"= "{{apiKey}}"



