#!/usr/bin/env dothttp

@name("READ settings")
GET "https://{{apiPath}}/environments/{{envID}}/brandingSettings"


@name("UPDATE settings")
PUT "https://{{apiPath}}/environments/{{envID}}/brandingSettings"
json({
    "companyName": "Ping Identity",
    "logo": {
        "href": "https://logo.com",
        "id": "00000000-0000-0000-0000-000000000000"
    }
})



