#!/usr/bin/env dothttp

@name("Get a label")
GET "https://{{baseUrl}}/repos/:owner/:repo/labels/:name"


@name("Update a label")
PATCH "https://{{baseUrl}}/repos/:owner/:repo/labels/:name"
"Content-Type": "application/json"
data('{
    "new_name": "<string>",
    "color": "<string>",
    "description": "<string>"
}')


@name("Delete a label")
DELETE "https://{{baseUrl}}/repos/:owner/:repo/labels/:name"



