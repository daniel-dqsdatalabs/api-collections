#!/usr/bin/env dothttp

// Fetches the URL to a migration archive.

@name("Download an organization migration archive")
GET "https://{{baseUrl}}/orgs/:org/migrations/:migration_id/archive"


// Deletes a previous migration archive. Migration archives are automatically deleted after seven days.

@name("Delete an organization migration archive")
DELETE "https://{{baseUrl}}/orgs/:org/migrations/:migration_id/archive"



