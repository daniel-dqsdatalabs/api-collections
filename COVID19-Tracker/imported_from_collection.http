#!/usr/bin/env dothttp

// Get the general stats for corona virus
@name("General Stats")
GET "https://corona-virus-stats.herokuapp.com/api/v1/cases/general-stats"


// Get all countries stats
@name("Country stats")
GET "https://corona-virus-stats.herokuapp.com/api/v1/cases/countries-search"
? "limit"= "8"
? "page"= "1"
? "search"= "ke"
? "order"= "total_cases"
? "how"= "asc"



