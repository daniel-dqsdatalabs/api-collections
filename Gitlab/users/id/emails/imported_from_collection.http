#!/usr/bin/env dothttp

// Get the emails addresses of a specified user. Available only for admins.
@name("Get Users  Emails")
GET "https://{{baseUrl}}/users/:id/emails"


// Delete an email address of a specified user. Available only for admins.
@name("Delete Users  Emails Email")
DELETE "https://{{baseUrl}}/users/:id/emails/:email_id"



