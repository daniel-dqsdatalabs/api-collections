#!/usr/bin/env dothttp

// Get a list of project +noteable+ notes

@name("Get Projects  Snippets Noteable  Notes")
GET "https://{{baseUrl}}/projects/:id/snippets/:noteable_id/notes"
? "page"= "<>"
? "per_page"= "<>"


// Delete projects  snippets noteable  notes note

@name("Delete Projects  Snippets Noteable  Notes Note")
DELETE "https://{{baseUrl}}/projects/:id/snippets/:noteable_id/notes/:note_id"



