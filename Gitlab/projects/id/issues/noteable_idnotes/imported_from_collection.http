#!/usr/bin/env dothttp

// Get a list of project +noteable+ notes
@name("Get Projects  Issues Noteable  Notes")
GET "https://{{baseUrl}}/projects/:id/issues/:noteable_id/notes"
? "page"= "<>"
? "per_page"= "<>"


// Delete projects  issues noteable  notes note
@name("Delete Projects  Issues Noteable  Notes Note")
DELETE "https://{{baseUrl}}/projects/:id/issues/:noteable_id/notes/:note_id"



