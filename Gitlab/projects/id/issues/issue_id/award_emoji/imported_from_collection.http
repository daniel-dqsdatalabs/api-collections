#!/usr/bin/env dothttp

// Get projects  issues issue  award emoji
@name("Get Projects  Issues Issue  Award Emoji")
GET "https://{{baseUrl}}/projects/:id/issues/:issue_id/award_emoji"
? "page"= "<>"
? "per_page"= "<>"


// Delete projects  issues issue  award emoji award
@name("Delete Projects  Issues Issue  Award Emoji Award")
DELETE "https://{{baseUrl}}/projects/:id/issues/:issue_id/award_emoji/:award_id"



