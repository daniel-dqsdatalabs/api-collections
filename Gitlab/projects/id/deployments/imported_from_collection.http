#!/usr/bin/env dothttp

// This feature was introduced in GitLab 8.11.
@name("Get Projects  Deployments")
GET "https://{{baseUrl}}/projects/:id/deployments"
? "page"= "<>"
? "per_page"= "<>"


// This feature was introduced in GitLab 8.11.
@name("Get Projects  Deployments Deployment")
GET "https://{{baseUrl}}/projects/:id/deployments/:deployment_id"



