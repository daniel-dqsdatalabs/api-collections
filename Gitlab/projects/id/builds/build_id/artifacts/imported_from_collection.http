#!/usr/bin/env dothttp

// This feature was introduced in GitLab 8.5
@name("Get Projects  Builds Build  Artifacts")
GET "https://{{baseUrl}}/projects/:id/builds/:build_id/artifacts"


// Keep the artifacts to prevent them from being deleted
@name("Add Projects  Builds Build  Artifacts Keep")
POST "https://{{baseUrl}}/projects/:id/builds/:build_id/artifacts/keep"



