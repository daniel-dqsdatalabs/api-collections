#!/usr/bin/env dothttp

// Get a project builds

@name("Get Projects  Builds")
GET "https://{{baseUrl}}/projects/:id/builds"
? "scope"= "<>"
? "page"= "<>"
? "per_page"= "<>"


// Get projects  builds artifacts ref name download

@name("Get Projects  Builds Artifacts Ref Name Download")
GET "https://{{baseUrl}}/projects/:id/builds/artifacts/:ref_name/download"
? "job"= "<>"



