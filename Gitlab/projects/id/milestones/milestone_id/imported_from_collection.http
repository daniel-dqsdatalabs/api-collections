#!/usr/bin/env dothttp

// Get projects  milestones milestone
@name("Get Projects  Milestones Milestone")
GET "https://{{baseUrl}}/projects/:id/milestones/:milestone_id"


// Get all issues for a single project milestone
@name("Get Projects  Milestones Milestone  Issues")
GET "https://{{baseUrl}}/projects/:id/milestones/:milestone_id/issues"
? "page"= "<>"
? "per_page"= "<>"



