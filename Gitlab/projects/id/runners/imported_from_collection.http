#!/usr/bin/env dothttp

// Get runners available for project
@name("Get Projects  Runners")
GET "https://{{baseUrl}}/projects/:id/runners"
? "scope"= "<>"
? "page"= "<>"
? "per_page"= "<>"


// Delete projects  runners runner
@name("Delete Projects  Runners Runner")
DELETE "https://{{baseUrl}}/projects/:id/runners/:runner_id"



