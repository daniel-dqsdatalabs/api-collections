#!/usr/bin/env dothttp

// Get projects  merge requests merge request  notes note  award emoji

@name("Get Projects  Merge Requests Merge Request  Notes Note  Award Emoji")
GET "https://{{baseUrl}}/projects/:id/merge_requests/:merge_request_id/notes/:note_id/award_emoji"
? "page"= "<>"
? "per_page"= "<>"


// Delete projects  merge requests merge request  notes note  award emoji award

@name("Delete Projects  Merge Requests Merge Request  Notes Note  Award Emoji Award")
DELETE "https://{{baseUrl}}/projects/:id/merge_requests/:merge_request_id/notes/:note_id/award_emoji/:award_id"



