#!/usr/bin/env dothttp

// Get a groups list

@name("Get Groups")
GET "https://{{baseUrl}}/groups"
? "statistics"= "<get>"
? "all_available"= "<>"
? "search"= "<>"
? "order_by"= "<>"
? "sort"= "<>"
? "page"= "<>"
? "per_page"= "<>"


// Get list of owned groups for authenticated user

@name("Get Groups Owned")
GET "https://{{baseUrl}}/groups/owned"
? "page"= "<>"
? "per_page"= "<>"
? "statistics"= "<get>"



