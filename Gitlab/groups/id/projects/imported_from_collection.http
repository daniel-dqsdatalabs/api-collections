#!/usr/bin/env dothttp

// Get a list of projects in this group.
@name("Get Groups  Projects")
GET "https://{{baseUrl}}/groups/:id/projects"
? "archived"= "<get>"
? "visibility"= "<>"
? "search"= "<>"
? "order_by"= "<>"
? "sort"= "<>"
? "simple"= "<>"
? "page"= "<>"
? "per_page"= "<>"


// Transfer a project to the group namespace. Available only for admin.
@name("Add Groups  Projects Project")
POST "https://{{baseUrl}}/groups/:id/projects/:project_id"



