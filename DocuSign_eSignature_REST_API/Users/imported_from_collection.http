@name("Updates the user signature for a specified user.")
PUT "https://{{baseUrl}}/v2.1/accounts/{{accountId}}/users/{{userId}}/signatures/{{signatureId}}"
"Accept": "application/json"
"Authorization": "Bearer {{accessToken}}"
