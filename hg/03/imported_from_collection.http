#!/usr/bin/env dothttp

@name("1、查询标签列表")
GET "http://localhost/wx/web/tag/list"
files(
	("tagId", "1")
)


@name("2、查询标签详情")
GET "http://localhost/wx/web/tag/2"


@name("3、添加标签")
POST "http://localhost/wx/web/tag"
"Content-Type": "application/json"
data('{
        "tagId": 2,
        "tagName": "标签2222",
        "remark": "备222注",
        "delFlag": "0"
    }')


@name("4、修改标签")
PUT "http://localhost/wx/web/tag"
"Content-Type": "application/json"
data('{
        "tagId": 2,
        "tagName": "标签333",
        "remark": "备222注3333",
        "delFlag": "0"
    }')


@name("5、删除标签")
DELETE "http://localhost/wx/web/tag/2"



