#!/usr/bin/env dothttp

@name("Retrieves a single SystemWidePolicy identified by the supplied ID")
GET "https://{{baseUrl}}/security/v1/systemwidepolicys/:id"
"Authorization": "<string>"
"If-None-Match": "<string>"


@name("Creates or updates an SystemWidePolicy with a specific ID")
PUT "https://{{baseUrl}}/security/v1/systemwidepolicys/:id"
"Authorization": "<string>"
"Asynchronous": "<boolean>"
"Content-Type": "application/x-www-form-urlencoded"
data({
    "features": [
        "[\"<string>\",\"<string>\"]"
    ],
    "statements": [
        "[\"<string>\",\"<string>\"]"
    ],
    "expires": [
        "<dateTime>"
    ],
    "id": [
        "<string>"
    ],
    "name": [
        "<string>"
    ],
    "keywords": [
        "<string>"
    ],
    "lastModifiedBy": [
        "<string>"
    ],
    "lastModifiedByName": [
        "<string>"
    ],
    "lastModifiedDate": [
        "<dateTime>"
    ],
    "createdDate": [
        "<dateTime>"
    ],
    "createdBy": [
        "<string>"
    ],
    "createdByName": [
        "<string>"
    ],
    "lockedForDeletion": [
        "<boolean>"
    ],
    "isConfiguration": [
        "<boolean>"
    ],
    "isSealed": [
        "<boolean>"
    ],
    "systemTimestamp": [
        "<byte>"
    ],
    "existsInNoSQL": [
        "<boolean>"
    ]
})



