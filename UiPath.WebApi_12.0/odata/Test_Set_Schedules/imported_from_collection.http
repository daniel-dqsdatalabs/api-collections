#!/usr/bin/env dothttp

// OAuth required scopes: OR.TestSetSchedules or OR.TestSetSchedules.Read.
// 
// Required permissions: TestSetSchedules.View.
// 
// Responses:
// 200 Returns a list of test set execution schedules filtered with queryOptions
// 403 If the caller doesn't have permissions to view test set execution schedules

@name("Returns a list of test set execution schedules")
GET "https://{{baseUrl}}/odata/TestSetSchedules"
"X-UIPATH-OrganizationUnitId": "12575632"
? "$expand"= "esse sit qui"
? "$filter"= "esse sit qui"
? "$select"= "esse sit qui"
? "$orderby"= "esse sit qui"
? "$top"= "12575632"
? "$skip"= "12575632"
? "$count"= "true"


// OAuth required scopes: OR.TestSetSchedules or OR.TestSetSchedules.Write.
// 
// Required permissions: TestSetSchedules.Create.
// 
// Responses:
// 201 Returns the newly created test set execution schedule
// 403 If the caller doesn't have permissions to create test set execution schedules

@name("Creates a new test set execution schedule")
POST "https://{{baseUrl}}/odata/TestSetSchedules"
"X-UIPATH-OrganizationUnitId": "12575632"
"Content-Type": "application/json"
data('{
    "Name": "tempor deserunt ut in",
    "TestSetId": 67876852,
    "TimeZoneId": "Ut qui fugiat",
    "Enabled": false,
    "Description": "quis",
    "TestSetName": "est in ea",
    "TimeZoneIana": "id",
    "CalendarId": 96393600,
    "CalendarName": "aliquip ex sit esse proident",
    "CronExpression": "dolor incididunt",
    "CronDetails": "do culpa in",
    "CronSummary": "sunt eu nostrud",
    "NextOccurrence": "1943-11-07T09:28:33.420Z",
    "DisableDate": "2019-09-12T10:25:32.741Z",
    "ExternalJobKey": "ip",
    "Id": -66153415
}')


// OAuth required scopes: OR.TestSetSchedules or OR.TestSetSchedules.Write.
// 
// Required permissions: TestSetSchedules.Edit.

@name("Enables / disables a list of test set execution schedules.")
POST "https://{{baseUrl}}/odata/TestSetSchedules/UiPath.Server.Configuration.OData.SetEnabled"
"X-UIPATH-OrganizationUnitId": "12575632"
"Content-Type": "application/json"
? "$expand"= "esse sit qui"
? "$select"= "esse sit qui"
data('{
    "enabled": true,
    "testSetScheduleIds": [
        -17248384,
        75402906
    ]
}')



