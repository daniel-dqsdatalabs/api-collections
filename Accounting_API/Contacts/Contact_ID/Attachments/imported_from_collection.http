#!/usr/bin/env dothttp

@name("Allows you to retrieve, add and update contacts in a Xero organisation")
GET "https://{{baseUrl}}/Contacts/:ContactID/Attachments"
"xero-tenant-id": "<string>"


@name("Allows you to retrieve Attachments on Contacts")
GET "https://{{baseUrl}}/Contacts/:ContactID/Attachments/:AttachmentID"
"contentType": "<string>"
"xero-tenant-id": "<string>"



