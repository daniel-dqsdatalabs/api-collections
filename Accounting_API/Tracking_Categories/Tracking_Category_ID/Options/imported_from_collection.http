#!/usr/bin/env dothttp

@name("Allows you to create options for a specified tracking category")
PUT "https://{{baseUrl}}/TrackingCategories/:TrackingCategoryID/Options"
"xero-tenant-id": "<string>"
"Content-Type": "application/json"
data('{
    "TrackingOptionID": "<uuid>",
    "Name": "<string>",
    "Status": "<string>",
    "TrackingCategoryID": "<uuid>"
}')



