#!/usr/bin/env dothttp

@name("Allows you to retrieve attachments for purchase orders")
GET "https://{{baseUrl}}/PurchaseOrders/:PurchaseOrderID/Attachments"
"xero-tenant-id": "<string>"


@name("Allows you to retrieve specific Attachment on purchase order")
GET "https://{{baseUrl}}/PurchaseOrders/:PurchaseOrderID/Attachments/:AttachmentID"
"contentType": "<string>"
"xero-tenant-id": "<string>"



