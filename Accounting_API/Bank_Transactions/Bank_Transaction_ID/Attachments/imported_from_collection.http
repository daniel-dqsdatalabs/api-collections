#!/usr/bin/env dothttp

@name("Allows you to retrieve any attachments to bank transactions")
GET "https://{{baseUrl}}/BankTransactions/:BankTransactionID/Attachments"
"xero-tenant-id": "<string>"


@name("Allows you to retrieve Attachments on a specific BankTransaction")
GET "https://{{baseUrl}}/BankTransactions/:BankTransactionID/Attachments/:AttachmentID"
"contentType": "<string>"
"xero-tenant-id": "<string>"



