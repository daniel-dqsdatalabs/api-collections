#!/usr/bin/env dothttp

@name("Allows you to retrieve Attachments on invoices or purchase bills")
GET "https://{{baseUrl}}/Invoices/:InvoiceID/Attachments"
"xero-tenant-id": "<string>"


@name("Allows you to retrieve a specified Attachment on invoices or purchase bills by it's ID")
GET "https://{{baseUrl}}/Invoices/:InvoiceID/Attachments/:AttachmentID"
"contentType": "<string>"
"xero-tenant-id": "<string>"



