#!/usr/bin/env dothttp

@name("Allows you to retrieve Attachments on repeating invoice")
GET "https://{{baseUrl}}/RepeatingInvoices/:RepeatingInvoiceID/Attachments"
"xero-tenant-id": "<string>"


@name("Allows you to retrieve a specified Attachments on repeating invoices")
GET "https://{{baseUrl}}/RepeatingInvoices/:RepeatingInvoiceID/Attachments/:AttachmentID"
"contentType": "<string>"
"xero-tenant-id": "<string>"



