#!/usr/bin/env dothttp

@name("Allows you to retrieve Attachments for Quotes")
GET "https://{{baseUrl}}/Quotes/:QuoteID/Attachments"
"xero-tenant-id": "<string>"


@name("Allows you to retrieve specific Attachment on Quote")
GET "https://{{baseUrl}}/Quotes/:QuoteID/Attachments/:AttachmentID"
"contentType": "<string>"
"xero-tenant-id": "<string>"



