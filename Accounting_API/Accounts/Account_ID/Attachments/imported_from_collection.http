#!/usr/bin/env dothttp

@name("Allows you to retrieve Attachments for accounts")
GET "https://{{baseUrl}}/Accounts/:AccountID/Attachments"
"xero-tenant-id": "<string>"


@name("Allows you to retrieve specific Attachment on Account")
GET "https://{{baseUrl}}/Accounts/:AccountID/Attachments/:AttachmentID"
"contentType": "<string>"
"xero-tenant-id": "<string>"



