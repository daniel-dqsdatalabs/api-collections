#!/usr/bin/env dothttp

@name("Allows you to retrieve any journals.")
GET "https://{{baseUrl}}/Journals"
"If-Modified-Since": "<dateTime>"
"xero-tenant-id": "<string>"
? "offset"= "<integer>"
? "paymentsOnly"= "<boolean>"


@name("Allows you to retrieve a specified journals.")
GET "https://{{baseUrl}}/Journals/:JournalID"
"xero-tenant-id": "<string>"



