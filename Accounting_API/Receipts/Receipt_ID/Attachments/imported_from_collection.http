#!/usr/bin/env dothttp

@name("Allows you to retrieve Attachments for expense claim receipts")
GET "https://{{baseUrl}}/Receipts/:ReceiptID/Attachments"
"xero-tenant-id": "<string>"


@name("Allows you to retrieve Attachments on expense claim receipts by ID")
GET "https://{{baseUrl}}/Receipts/:ReceiptID/Attachments/:AttachmentID"
"contentType": "<string>"
"xero-tenant-id": "<string>"



