#!/usr/bin/env dothttp

// This endpoint retrieves a list of all ATMs in a bank. // // `This endpoint supports paging`
@name("Get All ATMs")
GET "https://{{url}}/v1/atms"


// This endpoints returns the data of a specific ATM by specifying the terminal id.// // #### URL Parameters// // | Parameter |Type |Description  |// |--|--|--|// | terminalID|string |`Required` This is a terminal ID of the ATM you want to get|
@name("Get a Specific ATM")
GET "https://{{url}}/v1/atms/{{terminalId}}"


// This endpoints retrieves ATMs tied to specific branch identified by its branch ID. You can get branch IDs from `/v1/branch/`// // #### URL Parameters// // | Parameter |Type |Description  |// |--|--|--|// | branchID|string |`Required` This is a ID of the branch whose ATMs you want to get|
@name("Get ATM by Branch")
GET "https://{{url}}/v1/atms/branch/{branchId}"


// This endpoint gets ATMs within a geographic location specified by the longitude and latitude.// // `This endpoint supports paging`// // #### Query Parameters// | Parameter|Type |Description  |// |--|--|--|// | longitude |Decimal |`Required`  geographic coordinates|// | latitude |Decimal |`Required`  geographic coordinates|// | radius|integer |`Optional`   circular distance, in meters, around the co-ordinate|
@name("Get ATM by Location")
GET "https://{{url}}/v1/atms"
"": ""
? "latitude"= "{{latitude}}"
? "longitude"= "{{longitude}}"
? "radius"= "{{radius}}"



