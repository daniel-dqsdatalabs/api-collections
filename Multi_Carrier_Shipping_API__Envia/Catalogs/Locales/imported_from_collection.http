#!/usr/bin/env dothttp

@name("get all locales")
GET "http://{{queries_url}}.envia.com/locale"


// Get locale by country. <br>
// 
// <strong>US</strong>- United States<br>
// <strong>MX</strong>- Mexico<br>
// <strong>CO</strong>- Colombia<br>

@name("get single locale")
GET "http://{{queries_url}}.envia.com/locale/{{country_code}}"



