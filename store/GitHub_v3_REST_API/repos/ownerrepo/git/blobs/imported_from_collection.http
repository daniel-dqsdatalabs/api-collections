#!/usr/bin/env dothttp

# {{baseUrl=api.github.com}}

@name("Create a blob")
POST "https://{{baseUrl}}/repos/{{owner}}/{{repo/git/blobs}}/"
"Content-Type": "application/json"
json({
    "content": "Content of the blob",
    "encoding": "utf-8"
})


@name("Get a blob")
GET "https://{{baseUrl}}/repos/{{owner}}/{{repo/git/blobs/:file_sha}}/"



