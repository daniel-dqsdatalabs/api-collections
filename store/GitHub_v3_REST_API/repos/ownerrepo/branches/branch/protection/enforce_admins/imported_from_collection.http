#!/usr/bin/env dothttp

# {{baseUrl=api.github.com}}

@name("Get admin branch protection")
GET "https://{{baseUrl}}/repos/{{owner}}/{{repo/branches/{{branch}}/protection/enforce_admins}}/"


@name("Set admin branch protection")
POST "https://{{baseUrl}}/repos/{{owner}}/{{repo/branches/{{branch}}/protection/enforce_admins}}/"


@name("Delete admin branch protection")
DELETE "https://{{baseUrl}}/repos/{{owner}}/{{repo/branches/{{branch}}/protection/enforce_admins}}/"



