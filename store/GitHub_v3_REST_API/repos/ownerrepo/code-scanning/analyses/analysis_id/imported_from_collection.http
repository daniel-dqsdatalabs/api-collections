#!/usr/bin/env dothttp

# {{baseUrl=api.github.com}}

@name("Get a code scanning analysis for a repository")
GET "https://{{baseUrl}}/repos/{{owner}}/{{repo/code-scanning/analyses/:analysis_id}}/"


@name("Delete a code scanning analysis from a repository")
DELETE "https://{{baseUrl}}/repos/{{owner}}/{{repo/code-scanning/analyses/:analysis_id}}/"
? "confirm_delete"= "sed anim consectetur ea in"



