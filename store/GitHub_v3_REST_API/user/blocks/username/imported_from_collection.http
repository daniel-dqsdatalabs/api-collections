#!/usr/bin/env dothttp

# {{baseUrl=api.github.com}}

@name("Check if a user is blocked by the authenticated user")
GET "https://{{baseUrl}}/user/blocks/{{username}}/"


@name("Block a user")
PUT "https://{{baseUrl}}/user/blocks/{{username}}/"


@name("Unblock a user")
DELETE "https://{{baseUrl}}/user/blocks/{{username}}/"



