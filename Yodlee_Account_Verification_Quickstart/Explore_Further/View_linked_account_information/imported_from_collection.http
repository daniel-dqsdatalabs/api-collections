#!/usr/bin/env dothttp

// This call returns an array of provider information.  Pass a provider ID to limit the results to specific institution.

@name("View linked providers")
GET "https://{{url}}/providerAccounts"
"Api-Version": "1.1"
"Content-Type": "application/json"
"Authorization": "Bearer {{token}}"
? "providerIds"= "{{providerId}}"




