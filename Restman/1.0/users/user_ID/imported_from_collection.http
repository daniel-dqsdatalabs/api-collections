#!/usr/bin/env dothttp

// Returns a user with the given ID.
// Get User

@name("List Users By UserID")
GET "https://{{baseUrl}}/1.0/users/:userID"


// Change this user's password
// Change User Password

@name("Change Users Password By UserID")
PUT "https://{{baseUrl}}/1.0/users/:userID/password"
? "format"= "<string>"


// Gets the user's certificate
// Get User Certificate

@name("List Users Certificate By UserID")
GET "https://{{baseUrl}}/1.0/users/:userID/certificate"



