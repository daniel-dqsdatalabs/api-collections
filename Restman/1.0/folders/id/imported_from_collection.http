#!/usr/bin/env dothttp

// Returns a folder with the given ID.
// Get

@name("List Folders By ID")
GET "https://{{baseUrl}}/1.0/folders/:id"


// Returns the list of dependencies for this entity.
// Get Dependencies
// Get Dependencies

@name("List Folders Dependencies By ID")
GET "https://{{baseUrl}}/1.0/folders/:id/dependencies"
? "level"= "<integer>"



