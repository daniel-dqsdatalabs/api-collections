#!/usr/bin/env dothttp

// Returns a service with the given ID.
// Get

@name("List Services By ID")
GET "https://{{baseUrl}}/1.0/services/:id"


// Returns the list of dependencies for this entity.
// Get Dependencies
// Get Dependencies

@name("List Services Dependencies By ID")
GET "https://{{baseUrl}}/1.0/services/:id/dependencies"
? "level"= "<integer>"



