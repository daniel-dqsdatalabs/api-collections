#!/usr/bin/env dothttp

// Returns an encapsulated assertion with the given ID.
// Get

@name("List Encapsulated Assertions By ID")
GET "https://{{baseUrl}}/1.0/encapsulatedAssertions/:id"


// Returns the list of dependencies for this entity.
// Get Dependencies
// Get Dependencies

@name("List Encapsulated Assertions Dependencies By ID")
GET "https://{{baseUrl}}/1.0/encapsulatedAssertions/:id/dependencies"
? "level"= "<integer>"



