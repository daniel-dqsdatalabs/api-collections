#!/usr/bin/env dothttp

// Returns a policy with the given ID.
// Get

@name("List Policies By ID")
GET "https://{{baseUrl}}/1.0/policies/:id"


// Returns the list of dependencies for this entity.
// Get Dependencies
// Get Dependencies

@name("List Policies Dependencies By ID")
GET "https://{{baseUrl}}/1.0/policies/:id/dependencies"
? "level"= "<integer>"



