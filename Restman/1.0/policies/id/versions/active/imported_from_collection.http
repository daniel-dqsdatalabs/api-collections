#!/usr/bin/env dothttp

// Returns the active policy version.
// Get Active Version

@name("List Policies Versions Active By ID")
GET "https://{{baseUrl}}/1.0/policies/:id/versions/active"


// Sets a comment on the active policy version
// Set Active Version Comment

@name("Sets Policies Versions Active Comment By ID")
PUT "https://{{baseUrl}}/1.0/policies/:id/versions/active/comment"



