#!/usr/bin/env dothttp

// Returns a Log Sink Configuration with the given ID.
// Get

@name("List Log Sinks By ID")
GET "https://{{baseUrl}}/1.0/logSinks/:id"


// Returns the list of dependencies for this entity.
// Get Dependencies
// Get Dependencies

@name("List Log Sinks Dependencies By ID")
GET "https://{{baseUrl}}/1.0/logSinks/:id/dependencies"
? "level"= "<integer>"



