#!/usr/bin/env dothttp

@name("Single user by ID")
GET "https://api.twitter.com/labs/2/users/:id"


@name("Multiple users")
GET "https://api.twitter.com/labs/2/users"


@name("Single user by username")
GET "https://api.twitter.com/labs/2/users/by/username/:username"


@name("Multiple users by username")
GET "https://api.twitter.com/labs/2/users/by"


@name("Single Tweet")
GET "https://api.twitter.com/labs/2/tweets/:id"


@name("Multiple Tweets")
GET "https://api.twitter.com/labs/2/tweets"



