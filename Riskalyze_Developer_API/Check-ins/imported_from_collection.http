#!/usr/bin/env dothttp

// Get a list of check-ins.

@name("Get Check-ins List")
GET "https://{{base_url}}/ap/v1/checkins"
"Authorization": "Bearer {{access_token}}"
"Content-Type": "application/json"
? "list"= "1"


// Get a specific Check-in using it's ID.

@name("Get Check-in by ID")
GET "https://{{base_url}}/ap/v1/checkins/:id"
"Authorization": "Bearer {{access_token}}"
"Content-Type": "application/json"



