#!/usr/bin/env dothttp

@name("accounts")
GET "https://ads-api.twitter.com/{{version}}/accounts"


@name("accounts/:account_id")
GET "https://ads-api.twitter.com/{{version}}/accounts/{{account_id}}"


@name("accounts")
POST "https://ads-api.twitter.com/{{version}}/accounts"


@name("accounts/:account_id")
PUT "https://ads-api.twitter.com/{{version}}/accounts/{{account_id}}"


@name("accounts/:account_id")
DELETE "https://ads-api.twitter.com/{{version}}/accounts/{{account_id}}"



