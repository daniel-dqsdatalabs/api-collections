#!/usr/bin/env dothttp

@name("accounts/:account_id/account_media")
GET "https://ads-api.twitter.com/{{version}}/accounts/{{account_id}}/account_media"


@name("accounts/:account_id/account_media/:account_media_id")
GET "https://ads-api.twitter.com/{{version}}/accounts/{{account_id}}/account_media/:account_media_id"


@name("accounts/:account_id/account_media/:account_media_id")
DELETE "https://ads-api.twitter.com/{{version}}/accounts/{{account_id}}/account_media/:account_media_id"



