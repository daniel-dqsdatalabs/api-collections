#!/usr/bin/env dothttp

// Fetch and return Twitter data from MongoDB connection.
// 
// :param: none
// :return: str

@name("Get Twitter")
GET "https://{{baseUrl}}/twitter"


// Fetch and return Twitter data from MongoDB connection.
// 
// :param: none. Two letter state abbreviation.
// :return: str

@name("Post Twitter")
POST "https://{{baseUrl}}/twitter"
"Content-Type": "application/json"
data('{
    "state": "CA"
}')



