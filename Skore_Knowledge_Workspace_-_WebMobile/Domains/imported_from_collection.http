#!/usr/bin/env dothttp

@name("Show configs")
GET "https://{{KNOWLEDGE_BASE_URL}}/workspace/v1/domains/web/settings"
? "domain"= "skore.staging.skore.io"


@name("Show features")
GET "https://{{KNOWLEDGE_BASE_URL}}/workspace/v1/domains"
"authorization": "{{TOKEN}}"
? "name"= "web"


@name("By platform")
GET "https://{{host}}/"



