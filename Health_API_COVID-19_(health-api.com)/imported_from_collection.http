#!/usr/bin/env dothttp

// Aggregated statistics.
@name("Total")
GET "https://health-api.com/api/v1/covid-19/total"


// List of all countries and their provinces/states.
@name("Countries")
GET "https://health-api.com/api/v1/covid-19/all"


// Single country. Province/state data aggregated.
@name("County")
GET "https://health-api.com/api/v1/covid-19/:country"


// List of all provinces/states for a given country.
@name("States")
GET "https://health-api.com/api/v1/covid-19/:country/full"



