#!/usr/bin/env dothttp

// Takes a list of HTML tags, with different sizes applied, and turns into a visual tag cloud.

@name("Visualizer Tag Cloud")
POST "https://postman-echo.com/post"
"Content-Type": "application/json"
data('{
	"content":
	[
	 {{$randomInt}},
	 {{$randomInt}},
	 {{$randomInt}},
	 {{$randomInt}}
		
	]
}')



