#!/usr/bin/env dothttp

# imported from http://localhost:8000/Microsoft%20Graph.postman_collection.json

@name("Get Planner Tasks")
GET "<dotextensions.server.postman2_1.URLClass object at 0x7f028c863cd0>"
"Content-Type": "application/json"


@name("All Planner Tasks for a User")
GET "<dotextensions.server.postman2_1.URLClass object at 0x7f028c863e80>"
"Content-Type": "application/json"


@name("Get all planner plans associated with group")
GET "<dotextensions.server.postman2_1.URLClass object at 0x7f028c9a2070>"
"Content-Type": "application/json"


@name("Get Plan")
GET "<dotextensions.server.postman2_1.URLClass object at 0x7f028c9a2220>"
"Content-Type": "application/json"


@name("Update Plan")
PATCH "<dotextensions.server.postman2_1.URLClass object at 0x7f028c9a24c0>"
"Content-Type": "application/json"
"If-Match": "{{PlanETag}}"
data('{
  "title": "Updated plan title"
}')


@name("Get Plan Buckets")
GET "<dotextensions.server.postman2_1.URLClass object at 0x7f028c9a2610>"
"Content-Type": "application/json"


@name("Create a bucket in a Plan")
POST "<dotextensions.server.postman2_1.URLClass object at 0x7f028c9a28b0>"
"Content-Type": "application/json"
"If-Match": "{{PlanETag}}"
data('{
  "name": "New bucket",
  "planId": "{{PlanId}}",
  "orderHint": " !"
}')


@name("Update a bucket")
PATCH "<dotextensions.server.postman2_1.URLClass object at 0x7f028c9a2af0>"
"Content-Type": "application/json"
"If-Match": "{{PlanETag}}"
data('{
  "name": "Updated bucket name"
}')


@name("All Planner Tasks for a Plan")
GET "<dotextensions.server.postman2_1.URLClass object at 0x7f028c9a2c40>"
"Content-Type": "application/json"


@name("Get Planner Task by Id")
GET "<dotextensions.server.postman2_1.URLClass object at 0x7f028c9a2df0>"
"Content-Type": "application/json"


@name("Create a Planner Task")
POST "<dotextensions.server.postman2_1.URLClass object at 0x7f028c9a60d0>"
"Content-Type": "application/json"
data('{
  "planId": "{{PlanId}}",
  "title": "New Task",
  "assignments": {}
}')


@name("Update a Planner Task")
PATCH "<dotextensions.server.postman2_1.URLClass object at 0x7f028c9a62b0>"
"Content-Type": "application/json"
"If-Match": "{{PlanETag}}"
data('{
  "title": "Updated task title"
}')


@name("Get Planner Task details")
GET "<dotextensions.server.postman2_1.URLClass object at 0x7f028c9a6400>"
"Content-Type": "application/json"



