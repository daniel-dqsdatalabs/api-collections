#!/usr/bin/env dothttp

// Fetch director's details using DIN

@name("Fetch Director Details")
GET "https://{{sandbox_host}}/mca/directors/:din"
"Authorization": "{{access_token}}"
"x-api-key": "{{api_key}}"
"x-api-version": "{{version}}"


// Fetch companies's details using CIN or LLPIN

@name("Fetch Company Details")
GET "https://{{sandbox_host}}/mca/companies/:id"
"Authorization": "{{access_token}}"
"x-api-key": "{{api_key}}"
"x-api-version": "{{version}}"



