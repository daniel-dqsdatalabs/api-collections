#!/usr/bin/env dothttp

// Gives [Aadhaar entity](#aadhaar-entity) for valid Aadhaar number

@name("Aadhaar Verfication")
POST "https://{{host}}/aadhaar/verify"
"Authorization": "{{access_token}}"
"x-api-key": "{{api_key}}"
"x-api-version": "{{version}}"
json({
    "aadhaar_number": "543298761234"
})



