#!/usr/bin/env dothttp

@name("Generate OTP")
GET "https://{{host}}/gst-portal/tax-payers/:gstin/otp"
"Authorization": "{{access_token}}"
"x-api-key": "{{api_key}}"
"x-api-version": "{{version}}"


@name("Verify OTP")
POST "https://{{host}}/gst-portal/tax-payers/:gstin/otp"
"Authorization": "{{access_token}}"
"x-api-key": "{{api_key}}"
"x-api-version": "{{version}}"



