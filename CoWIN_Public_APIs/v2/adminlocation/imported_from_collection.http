#!/usr/bin/env dothttp

// API to get all the states in India.
@name("Get states")
GET "https://{{baseUrl}}/v2/admin/location/states"
"Accept-Language": "hi_IN"


// API to get all the districts.
@name("Get list of districts")
GET "https://{{baseUrl}}/v2/admin/location/districts/:state_id"
"Accept-Language": "hi_IN"



