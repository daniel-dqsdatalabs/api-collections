#!/usr/bin/env dothttp

// Use this api call to obtain the latest version of the app

@name("APP version")
GET "https://{{host}}/api/v1/platform_settings/app_version"


// Returns links to the iris arke legal documents

@name("Legal documents")
GET "https://{{host}}/api/v1/general/legal_documents"
"X-Spree-Token": "{{api_key}}"
"Accept": "application/json"
"Content-Type": "application/json"



