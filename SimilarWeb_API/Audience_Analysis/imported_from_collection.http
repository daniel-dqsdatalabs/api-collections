#!/usr/bin/env dothttp

// Returns a breakdown of age groups for traffic visiting the domain

@name("Desktop Demographics - Age")
GET "https://api.similarweb.com/v1/website/bbc.com/demographics/age"
? "api_key"= "{{similarweb_api_key}}"
? "start_date"= "2020-01"
? "end_date"= "2020-03"
? "country"= "gb"
? "main_domain_only"= "false"
? "format"= "json"


// Returns a breakdown of gender for traffic visiting the domain

@name("Desktop Demographics - Gender")
GET "https://api.similarweb.com/v1/website/bbc.com/demographics/gender"
? "api_key"= "{{similarweb_api_key}}"
? "start_date"= "2020-01"
? "end_date"= "2020-03"
? "country"= "gb"
? "main_domain_only"= "false"
? "format"= "json"


