#!/usr/bin/env dothttp

// Gets remaining hits, allowed country filters and available time periods for your account.
// 
// This endpoint does not cost any hits.

@name("Check Capabilities")
GET "https://api.similarweb.com/capabilities"
? "api_key"= "{{similarweb_api_key}}"


// Returns list of all SimilarWeb site categories

@name("List Categories")
GET "https://api.similarweb.com/v1/TopSites/categories"



