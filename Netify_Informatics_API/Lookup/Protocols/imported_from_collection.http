#!/usr/bin/env dothttp

// Get a list of all protocols supported by the Netify DPI engine.

@name("Get protocols")
GET "https://{{URL_INFORMATICS}}/api/{{API_VERSION}}/lookup/protocols"
? "page"= "1"


// Get metadata on a specific protocol.

@name("Get protocol")
GET "https://{{URL_INFORMATICS}}/api/{{API_VERSION}}/lookup/protocols/:id"



