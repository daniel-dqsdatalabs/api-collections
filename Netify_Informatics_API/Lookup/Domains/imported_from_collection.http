#!/usr/bin/env dothttp

// Get a list of domains.

@name("Get domains")
GET "https://{{URL_INFORMATICS}}/api/v2/lookup/domains"
? "page"= "1"
? "settings_limit"= "5"
? "filter_starts_with"= "twitter"


// Get metadata on a specific domain name.

@name("Get domain")
GET "https://{{URL_INFORMATICS}}/api/v2/lookup/domains/:domain"



