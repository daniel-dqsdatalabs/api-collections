#!/usr/bin/env dothttp

// Returns external flow data with latitude and longitude information.

@name("Get mapstream")
GET "https://{{URL_INFORMATICS}}/api/{{API_VERSION}}/data/geolocation/mapstream"
"x-net-site": "{{SITE_UUID}}"
"x-api-key": "{{API_KEY}}"
? 'filter_required_fields'= '["hostname"]'



