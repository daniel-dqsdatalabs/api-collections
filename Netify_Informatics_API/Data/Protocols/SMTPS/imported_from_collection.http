#!/usr/bin/env dothttp

// Returns top domain categories data.

@name("Get top categories")
GET "https://{{URL_INFORMATICS}}/api/{{API_VERSION}}/data/protocols/smtps/top_categories"
"x-net-site": "{{SITE_UUID}}"
"x-api-key": "{{API_KEY}}"


// Returns top domains data.

@name("Get top domains")
GET "https://{{URL_INFORMATICS}}/api/{{API_VERSION}}/data/protocols/smtps/top_domains"
"x-net-site": "{{SITE_UUID}}"
"x-api-key": "{{API_KEY}}"


// Return data on top hostnames.

@name("Get top hostnames")
GET "https://{{URL_INFORMATICS}}/api/{{API_VERSION}}/data/protocols/smtps/top_hostnames"
"x-net-site": "{{SITE_UUID}}"
"x-api-key": "{{API_KEY}}"


// Return TLS security status.

@name("Get TLS security status")
GET "https://{{URL_INFORMATICS}}/api/{{API_VERSION}}/data/protocols/smtps/tls_security/statusboard"
"x-net-site": "{{SITE_UUID}}"
"x-api-key": "{{API_KEY}}"


// Return TLS version status.

@name("Get TLS version status")
GET "https://{{URL_INFORMATICS}}/api/{{API_VERSION}}/data/protocols/smtps/tls_security/statusboard"
"x-net-site": "{{SITE_UUID}}"
"x-api-key": "{{API_KEY}}"



