#!/usr/bin/env dothttp

@name("List all Social Media Accounts")
GET "https://{{gsa_base_url}}/social_media"
? "API_KEY"= "{{gsa_api_key}}"
? "q"= "disease"
? "agencies"= "<string>"
? "tags"= "<string>"
? "language"= "<string>"
? "page_size"= "100"
? "page"= "1"


@name("Info for a specific social media account")
GET "https://{{gsa_base_url}}/social_media/:socialMediaAccountId"
? "API_KEY"= "{{gsa_api_key}}"



