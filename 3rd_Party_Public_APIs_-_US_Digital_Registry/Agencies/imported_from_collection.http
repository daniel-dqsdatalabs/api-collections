#!/usr/bin/env dothttp

@name("Info for a specific agency by id")
GET "https://{{gsa_base_url}}/agencies/:agencyId"
? "API_KEY"= "{{gsa_api_key}}"


@name("Search for government agencies")
GET "https://{{gsa_base_url}}/agencies"
? "API_KEY"= "{{gsa_api_key}}"
? "q"= "disease"
? "no_accounts"= "<string>"
? "page_size"= "100"
? "page"= "1"



