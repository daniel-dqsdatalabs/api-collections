#!/usr/bin/env dothttp

@name("Create New Joke")
POST "https://tattijokes.herokuapp.com/create"
json({
    "title": "The Office",
    "jokes": "Thats what she said"
})


@name("READ the Joke")
GET "https://tattijokes.herokuapp.com/read"


@name("Update the Joke")
POST "https://tattijokes.herokuapp.com/update"
json({
    "id": 57,
    "jokes": "Hello bea'chesss"
})


@name("Delete the Joke")
POST "https://tattijokes.herokuapp.com/delete"
json({
    "id": 57
})



