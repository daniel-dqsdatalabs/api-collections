#!/usr/bin/env dothttp

// List all organizations visible by your account.// // https://developer.webex.com/endpoint-organizations-get.html// // Example Response:// ``` json// {//   'items' : [ {//     'id' : 'OTZhYmMyYWEtM2RjYy0xMWU1LWExNTItZmUzNDgxOWNkYzlh',//     'displayName' : 'Acme, Inc.',//     'created' : '2015-10-18T14:26:16+00:00'//   } ]// }// ```
@name("List Organizations")
GET "https://{{WEBEXAPI}}/organizations"
"Authorization": "Bearer {{access_token}}"


// Shows details for an organization, by ID.// // Specify the org ID in the orgId parameter in the URI.// // https://developer.webex.com/endpoint-organizations-orgId-get.html// // Example Response:// ``` json// {//   'id' : 'OTZhYmMyYWEtM2RjYy0xMWU1LWExNTItZmUzNDgxOWNkYzlh',//   'displayName' : 'Acme, Inc.',//   'created' : '2015-10-18T14:26:16+00:00'// }// ```
@name("Get Organization Details")
GET "https://{{WEBEXAPI}}/organizations/{{_organization}}"
"Authorization": "Bearer {{access_token}}"



