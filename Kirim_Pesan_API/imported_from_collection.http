// This request will be remove token
// 
// Note :
//  - You must have token from Login Request to use this request

@name("Logout")
POST "https://apiks.ristekmuslim.com/public/v1/client/logout"
"Authorization": "Bearer <token-from-login>"
"Content-Type": "application/json"



// This request for get message info like status, WhatsApp message id and others.

@name("Message Info")
GET "https://apiks.ristekmuslim.com/client/v1/message/<messageid>"
"Authorization": "Bearer  <token-from-login>"


// This request for get instance info like status login, device info and others.

@name("Instance Info")
GET "https://apiks.ristekmuslim.com/client/v1/instance/<instance id>"
"Authorization": "Bearer <token>"


// Webhook request to the your server when receive incoming message
// 
// type of message :
// "text"
// "location" with thumbnail location
// "image" with file image,
// "video" with file audio,
// "audio" with file audio,
// "document" with file document,
// "sticker" with file sitcker,
// "contact" with vcard info

@name("Webhook")
POST "https://example.com/your-webhook"
"Content-Type": "application/json"
json({
    "clientID": "your client id",
    "instanceID": "your instance id",
    "phone": "phone",
    "type": "text",
    "value": "value of type message",
    "waID": "wa id",
    "waMessageID": "wa message id",
    "status": 3,
    "statusInfo": "read",
    "createdAt": "2019-09-30T15:59:13.829+07:00"
})


// Check phone exist is method for check phone number is registered in WhatsApp or not.
// 
// Note :
//  - You must have token from Login Request to use this request

@name("Check Phone Exist")
POST "https://apiks.ristekmuslim.com/client/v1/phone/exist"
"Authorization": "Bearer <token-from-login>"
"Content-Type": "application/json"
json({
    "instanceID": "<your instance id>",
    "phone": "<phone number with internasional code, exmp: 62812345679>"
})



