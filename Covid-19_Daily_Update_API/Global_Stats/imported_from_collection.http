#!/usr/bin/env dothttp

// Infection, Death, and Cured arround the world.

@name("Get Global Stats")
GET "https://{{apiUrl}}/api/v1/world"
"Content-Type": "application/json"



@name("Get Global Stats By Continent")
GET "https://{{apiUrl}}/api/v1/world/continent/:continent"
"Content-Type": "application/json"



@name("Get Global Stats By Country")
GET "https://{{apiUrl}}/api/v1/world/country/:country"
"Content-Type": "application/json"




