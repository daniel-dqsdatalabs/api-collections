#!/usr/bin/env dothttp

@name("Get All Death Recorded")
GET "https://{{apiUrl}}/api/v1/death"
"Content-Type": "application/json"



// Use a valid graph name
// *	totalDeaths
// *	dailyDeaths
// *	dailyDeathsGrowthFactor

@name("Get Death Graph By Name")
GET "https://{{apiUrl}}/api/v1/death/graph/:death"
"Content-Type": "application/json"




