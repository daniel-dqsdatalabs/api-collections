#!/usr/bin/env dothttp

// Creates a new list.

@name("Create List")
POST "https://{{baseUrl}}/v2/lists"
"Content-Type": "application/x-www-form-urlencoded"
? "api_key"= "PRIVATE_KEY"
data({
    "list_name": [
        "MyNewList"
    ]
})


// Returns a listing of all of the lists in an account.

@name("Get Lists")
GET "https://{{baseUrl}}/v2/lists"
? "api_key"= "PRIVATE_KEY"



