#!/usr/bin/env dothttp

// Get province/s recorded in the database

@name("Province")
GET "https://{{base_url}}/province/get"


// Get city/s recorded in the database

@name("City")
GET "https://{{base_url}}/city/get"
? "province_id"= "ID-AC"
? "is_enabled"= "1"


// Get bank/s recorded in the database

@name("Bank")
GET "https://{{base_url}}/bank/get"
? "is_enable"= "1"


// Get page/s recorded in the database

@name("Page")
GET "https://{{base_url}}/post/get"



