#!/usr/bin/env dothttp

@name("Prompts the user for authorization using Digest Auth + Algorithm.")
GET "https://{{baseUrl}}/digest-auth/:qop/:user/:passwd/:algorithm"


// allow settings the stale_after argument.

@name("Prompts the user for authorization using Digest Auth + Algorithm.")
GET "https://{{baseUrl}}/digest-auth/:qop/:user/:passwd/:algorithm/:stale_after"



