#!/usr/bin/env dothttp

// This call will return the contacts configured within the account.

@name("Retrieve Contacts")
GET "https://api.sonar.constellix.com/rest/api/contacts"
"x-cns-security-token": "{{token}}"
"Content-Type": "application/json"


// This call returns the configured contact groups within the account.

@name("Retrieve Contact Groups")
GET "https://{{baseurl}}/api/contact/groups"
"x-cns-security-token": "{{token}}"
"Content-Type": "application/json"



