#!/usr/bin/env dothttp

// Returns a list of all supported integration types.

@name("LIst Integrations Types")
GET "https://{{api-endpoint}}/integration/type"
"accept": "application/json; charset=UTF-8"
"x-redlock-auth": "{{token}}"


// Returns all your integrations, optionally filtered by type.

@name("List All Integrations")
GET "https://{{api-endpoint}}/integration"
"accept": "application/json; charset=UTF-8"
"x-redlock-auth": "{{token}}"



