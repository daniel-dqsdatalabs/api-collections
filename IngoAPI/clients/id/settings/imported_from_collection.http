#!/usr/bin/env dothttp

@name("/clients/:id/settings")
PUT "https://{{baseUrl}}/clients/:id/settings"
"Content-Type": "application/json"
"x-api-key": "{{apiKey}}"
"Authorization": "<Authorization>"
json({
    "push_notifications": "variable booleana",
    "featured_promotions": "variable booleana",
    "favorite_shop_promotions": "variable booleana"
})


@name("/clients/:id/settings")
OPTIONS "https://{{baseUrl}}/clients/:id/settings"



