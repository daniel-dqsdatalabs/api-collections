#!/usr/bin/env dothttp

@name("send Help Request")
POST "https://{{baseUrl}}/help"
"Content-Type": "application/json"
"x-api-key": "{{apiKey}}"
"Authorization": "<Authorization>"
data('{
    "id_client": {{idClient}},
    "comment": "comentario"
}')


@name("/help")
OPTIONS "https://{{baseUrl}}/help"



