#!/usr/bin/env dothttp

@name("/campaigns/:id/items")
GET "https://{{baseUrl}}/campaigns/:id/items"
"x-api-key": "{{apiKey}}"
? "checkpoint_id"= "<checkpoint_id>"
? "forward"= "<forward>"
? "size"= "<size>"
? "id_client"= "{{idClient}}"


@name("/campaigns/:id/items")
OPTIONS "https://{{baseUrl}}/campaigns/:id/items"



