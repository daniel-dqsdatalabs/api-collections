#!/usr/bin/env dothttp

@name("/banners/:id/items")
GET "https://{{baseUrl}}/banners/:id/items"
"x-api-key": "{{apiKey}}"
? "checkpoint_id"= "<checkpoint id>"
? "forward"= "true"
? "size"= "<size>"
? "id_client"= "{{idClient}}"


@name("/banners/:id/items")
OPTIONS "https://{{baseUrl}}/banners/:id/items"



