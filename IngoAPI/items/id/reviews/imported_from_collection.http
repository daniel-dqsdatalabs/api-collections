#!/usr/bin/env dothttp

@name("add Item Review")
POST "https://{{baseUrl}}/items/:id/reviews"
"Content-Type": "application/json"
"x-api-key": "{{apiKey}}"
"Authorization": "<Authorization>"
? "id_client"= "{{idClient}}"
data('{
    "rating": "rating: int",
    "comment": "comentario"
}')


@name("/items/:id/reviews")
OPTIONS "https://{{baseUrl}}/items/:id/reviews"



