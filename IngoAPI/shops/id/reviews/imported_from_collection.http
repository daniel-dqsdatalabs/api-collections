#!/usr/bin/env dothttp

@name("add Shop Review")
POST "https://{{baseUrl}}/shops/:id/reviews"
"x-api-key": "{{apiKey}}"
"Authorization": "<Authorization>"
? "id_client"= "{{idClient}}"
? "rating"= "<rating>"


@name("/shops/:id/reviews")
PUT "https://{{baseUrl}}/shops/:id/reviews"
"x-api-key": "{{apiKey}}"
"Authorization": "<Authorization>"
? "rating"= "laborum"


@name("/shops/:id/reviews")
OPTIONS "https://{{baseUrl}}/shops/:id/reviews"



