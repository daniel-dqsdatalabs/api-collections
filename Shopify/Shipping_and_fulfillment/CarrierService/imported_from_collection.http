#!/usr/bin/env dothttp

// Creates a carrier service

@name("Creates a carrier service")
POST "https://{{store_name}}.myshopify.com/admin/api/{{api_version}}/carrier_services.json"
"Content-Type": "application/json"



// <p>Updates a carrier service. Only the app that creates a carrier service can update it.</p>

@name("Updates a carrier service")
PUT "https://{{store_name}}.myshopify.com/admin/api/{{api_version}}/carrier_services/{{carrier_service_id}}.json"
"Content-Type": "application/json"



// Retrieves a list of carrier services

@name("Retrieves a list of carrier services")
GET "https://{{store_name}}.myshopify.com/admin/api/{{api_version}}/carrier_services.json"
"Content-Type": "application/json"


// <p>Retrieves a single carrier service by its ID</p>

@name("Retrieves a single carrier service")
GET "https://{{store_name}}.myshopify.com/admin/api/{{api_version}}/carrier_services/{{carrier_service_id}}.json"
"Content-Type": "application/json"


// <p>Deletes a carrier service</p>

@name("Deletes a carrier service")
DELETE "https://{{store_name}}.myshopify.com/admin/api/{{api_version}}/carrier_services/{{carrier_service_id}}.json"
"Content-Type": "application/json"




