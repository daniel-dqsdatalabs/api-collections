#!/usr/bin/env dothttp

// Sends a fulfillment request to the fulfillment service of a fulfillment order.

@name("Sends a fulfillment request")
POST "https://{{store_name}}.myshopify.com/admin/api/{{api_version}}/fulfillment_orders/{{fulfillment_order_id}}/fulfillment_request.json"
"Content-Type": "application/json"



// Accepts a fulfillment request sent to a fulfillment service for a fulfillment order.

@name("Accepts a fulfillment request")
POST "https://{{store_name}}.myshopify.com/admin/api/{{api_version}}/fulfillment_orders/{{fulfillment_order_id}}/fulfillment_request/accept.json"
"Content-Type": "application/json"



// Rejects a fulfillment request sent to a fulfillment service for a fulfillment order.

@name("Rejects a fulfillment request")
POST "https://{{store_name}}.myshopify.com/admin/api/{{api_version}}/fulfillment_orders/{{fulfillment_order_id}}/fulfillment_request/reject.json"
"Content-Type": "application/json"




