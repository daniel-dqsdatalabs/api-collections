#!/usr/bin/env dothttp

// Retrieves a list of fulfillment events for a specific fulfillment

@name("Retrieves a list of fulfillment events for a specific fulfillment")
GET "https://{{store_name}}.myshopify.com/admin/api/{{api_version}}/orders/{{order_id}}/fulfillments/{{fulfillment_id}}/events.json"
"Content-Type": "application/json"


// Retrieves a specific fulfillment event

@name("Retrieves a specific fulfillment event")
GET "https://{{store_name}}.myshopify.com/admin/api/{{api_version}}/orders/{{order_id}}/fulfillments/{{fulfillment_id}}/events/{{event_id}}.json"
"Content-Type": "application/json"


// Creates a fulfillment event

@name("Creates a fulfillment event")
POST "https://{{store_name}}.myshopify.com/admin/api/{{api_version}}/orders/{{order_id}}/fulfillments/{{fulfillment_id}}/events.json"
"Content-Type": "application/json"



// Deletes a fulfillment event

@name("Deletes a fulfillment event")
DELETE "https://{{store_name}}.myshopify.com/admin/api/{{api_version}}/orders/{{order_id}}/fulfillments/{{fulfillment_id}}/events/{{event_id}}.json"
"Content-Type": "application/json"




