#!/usr/bin/env dothttp

// Retrieves a list of locations that a fulfillment order can potentially move to. The resulting list is sorted alphabetically in ascending order by location name.

@name("Retrieves a list of locations that a fulfillment order can potentially move to.")
GET "https://{{store_name}}.myshopify.com/admin/api/{{api_version}}/fulfillment_orders/{{fulfillment_order_id}}/locations_for_move.json"
"Content-Type": "application/json"



