#!/usr/bin/env dothttp

// Resource para obter os layouts de botões touchs disponíveis no sistema.

@name("Consultar layout")
GET "https://{{url}}/{{version}}/layouts/"
basicauth("super", "{{password}}")
"Authorization": "Basic c3VwZXI6MTIzNA=="
? "api_key"= "{{apikey}}"
? "layout_id"= "{{layout_id}}"
? "codigo"= "{{codigo}}"
? "ordem"= "{{ordem}}"
? "tipo_botao"= "{{tipo_botao}}"
? "layout_tipo"= "{{layout_tipo}}"




