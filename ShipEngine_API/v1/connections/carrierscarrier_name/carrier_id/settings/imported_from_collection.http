#!/usr/bin/env dothttp

// Get carrier settings

@name("Get carrier settings")
GET "https://{{baseUrl}}/v1/connections/carriers/:carrier_name/:carrier_id/settings"
"API-Key": "{{API_KEY}}"


// Update carrier settings

@name("Update carrier settings")
PUT "https://{{baseUrl}}/v1/connections/carriers/:carrier_name/:carrier_id/settings"
"API-Key": "{{API_KEY}}"
"Content-Type": "application/json"
data('""')



