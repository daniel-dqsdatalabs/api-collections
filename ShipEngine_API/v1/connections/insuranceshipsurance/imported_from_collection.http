#!/usr/bin/env dothttp

// Disconnect a Shipsurance Account

@name("Disconnect a Shipsurance Account")
DELETE "https://{{baseUrl}}/v1/connections/insurance/shipsurance"
"API-Key": "{{API_KEY}}"


// Connect a Shipsurance Account

@name("Connect a Shipsurance Account")
POST "https://{{baseUrl}}/v1/connections/insurance/shipsurance"
"API-Key": "{{API_KEY}}"
"Content-Type": "application/json"
data('""')



