#!/usr/bin/env dothttp

// List Batches associated with your Shipengine account

@name("List Batches")
GET "https://{{baseUrl}}/v1/batches"
"API-Key": "{{API_KEY}}"


// Create a Batch

@name("Create A Batch")
POST "https://{{baseUrl}}/v1/batches"
"API-Key": "{{API_KEY}}"
"Content-Type": "application/json"



// Get Batch By External ID

@name("Get Batch By External ID")
GET "https://{{baseUrl}}/v1/batches/external_batch_id/:external_batch_id"
"API-Key": "{{API_KEY}}"



