#!/usr/bin/env dothttp

// Add a tag to the shipment object

@name("Add Tag to Shipment")
POST "https://{{baseUrl}}/v1/shipments/:shipment_id/tags/:tag_name"
"API-Key": "{{API_KEY}}"


// Remove an existing tag from the Shipment object

@name("Remove Tag from Shipment")
DELETE "https://{{baseUrl}}/v1/shipments/:shipment_id/tags/:tag_name"
"API-Key": "{{API_KEY}}"



