#!/usr/bin/env dothttp

@name("/wc/v3/shipping_methods")
GET "https://{{baseUrl}}/wc/v3/shipping_methods"
basicauth("{{CLIENT_KEY}}", "{{CLIENT_SECRET}}")
? "context"= "view"
? "context"= "view"


@name("/wc/v3/shipping_methods/:id")
GET "https://{{baseUrl}}/wc/v3/shipping_methods/:id"
basicauth("{{CLIENT_KEY}}", "{{CLIENT_SECRET}}")
? "context"= "view"
? "context"= "view"



