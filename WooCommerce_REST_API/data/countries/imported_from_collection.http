#!/usr/bin/env dothttp

@name("/wc/v3/data/countries")
GET "https://{{baseUrl}}/wc/v3/data/countries"
basicauth("{{CLIENT_KEY}}", "{{CLIENT_SECRET}}")


@name("/wc/v3/data/countries/:location")
GET "https://{{baseUrl}}/wc/v3/data/countries/:location"
basicauth("{{CLIENT_KEY}}", "{{CLIENT_SECRET}}")



