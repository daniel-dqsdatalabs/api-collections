#!/usr/bin/env dothttp

@name("/wc/v3/data/currencies")
GET "https://{{baseUrl}}/wc/v3/data/currencies"
basicauth("{{CLIENT_KEY}}", "{{CLIENT_SECRET}}")


@name("/wc/v3/data/currencies/current")
GET "https://{{baseUrl}}/wc/v3/data/currencies/current"
basicauth("{{CLIENT_KEY}}", "{{CLIENT_SECRET}}")


@name("/wc/v3/data/currencies/:currency")
GET "https://{{baseUrl}}/wc/v3/data/currencies/:currency"
basicauth("{{CLIENT_KEY}}", "{{CLIENT_SECRET}}")
? "location"= "<string>"



