#!/usr/bin/env dothttp

@name("/wc/v3/data/continents")
GET "https://{{baseUrl}}/wc/v3/data/continents"
basicauth("{{CLIENT_KEY}}", "{{CLIENT_SECRET}}")


@name("/wc/v3/data/continents/:location")
GET "https://{{baseUrl}}/wc/v3/data/continents/:location"
basicauth("{{CLIENT_KEY}}", "{{CLIENT_SECRET}}")
? "continent"= "<string>"



