#!/usr/bin/env dothttp

// Creates a new section in a project.

@name("Post Project Sections")
POST "https://{{baseUrl}}/projects/:project_gid/sections"


// Move sections relative to each other in a board view. One of before_section or after_section is required.

@name("Post Project Sections Insert")
POST "https://{{baseUrl}}/projects/:project_gid/sections/insert"



