#!/usr/bin/env dothttp

@name("版本升级专用接口")
POST "https://{{base_url}}/reconnectCallBack"
"Content-Type": "application/json"
"Authorization": "{{authorization}}"
data('{
	"wId": "{{wId}}"
}')


@name("查询微信是否在线")
POST "https://{{base_url}}/isOnline"
"Content-Type": "application/json"
"Authorization": "{{authorization}}"
data('{
    "wId": "{{wId}}"
}')


@name("退出微控平台")
POST "https://{{base_url}}/member/logout"
"Content-Type": "application/json"
"Authorization": "{{authorization}}"




