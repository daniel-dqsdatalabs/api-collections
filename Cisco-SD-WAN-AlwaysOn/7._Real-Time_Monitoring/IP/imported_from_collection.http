? "deviceId"= "{{deviceId}}"


@name("Retrieve NAT interface statistics from device")
GET "https://{{vmanage}}/dataservice/device/ip/nat/interfacestatistics"
"X-XSRF-TOKEN": "{{X-XSRF-TOKEN}}"
