#!/usr/bin/env dothttp

@name("Retrieve users list logged in users")
GET "https://{{vmanage}}/dataservice/device/users/list"
"X-XSRF-TOKEN": "{{X-XSRF-TOKEN}}"
? "deviceId"= "{{deviceId}}"


@name("Retrieve list of users from device")
GET "https://{{vmanage}}/dataservice/device/users"
"X-XSRF-TOKEN": "{{X-XSRF-TOKEN}}"
? "deviceId"= "{{deviceId}}"



