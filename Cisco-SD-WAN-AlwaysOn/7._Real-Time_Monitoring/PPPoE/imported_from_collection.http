#!/usr/bin/env dothttp

@name("Retrieve PPPoE session list from device")
GET "https://{{vmanage}}/dataservice/device/pppoe/session"
"X-XSRF-TOKEN": "{{X-XSRF-TOKEN}}"
? "deviceId"= "{{deviceId}}"


@name("Retrieve PPPoE statistics list from device")
GET "https://{{vmanage}}/dataservice/device/pppoe/statistic"
"X-XSRF-TOKEN": "{{X-XSRF-TOKEN}}"
? "deviceId"= "{{deviceId}}"



