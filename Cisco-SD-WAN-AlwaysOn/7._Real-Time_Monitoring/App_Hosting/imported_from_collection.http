"X-XSRF-TOKEN": "{{X-XSRF-TOKEN}}"
? "deviceId"= "{{deviceId}}"


@name("Retrieve App Hosting Network Interfaces from device")
GET "https://{{vmanage}}/dataservice/device/app-hosting/network-interfaces"
"X-XSRF-TOKEN": "{{X-XSRF-TOKEN}}"
