@name("Retrieve App Hosting Guest Routes from device")
GET "https://{{vmanage}}/dataservice/device/app-hosting/guest-routes"
"X-XSRF-TOKEN": "{{X-XSRF-TOKEN}}"
? "deviceId"= "{{deviceId}}"



