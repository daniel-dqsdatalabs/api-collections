#!/usr/bin/env dothttp

@name("Retrieve App Log Flows from device")
GET "https://{{vmanage}}/dataservice/device/app/log/flows"
"X-XSRF-TOKEN": "{{X-XSRF-TOKEN}}"
? "deviceId"= "{{deviceId}}"


@name("Retrieve App Log Flow counts from device")
GET "https://{{vmanage}}/dataservice/device/app/log/flow-count"
"X-XSRF-TOKEN": "{{X-XSRF-TOKEN}}"
? "deviceId"= "{{deviceId}}"



