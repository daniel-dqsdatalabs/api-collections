#!/usr/bin/env dothttp

@name("Fabric Devices")
GET "https://{{vmanage}}/dataservice/device"
"X-XSRF-TOKEN": "{{X-XSRF-TOKEN}}"


@name("Devices Status")
GET "https://{{vmanage}}/dataservice/device/monitor"
