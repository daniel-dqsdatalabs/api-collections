#!/usr/bin/env dothttp

@name("Returns a list of salesQuotes")
GET "https://{{baseUrl}}/companies({{company_id}})/salesQuotes"
? "$top"= "<integer>"
? "$skip"= "<integer>"
? "$limit"= "<integer>"
? "$filter"= "<string>"
? "$expand"= "<string>,<string>"
? "$select"= "<string>,<string>"


