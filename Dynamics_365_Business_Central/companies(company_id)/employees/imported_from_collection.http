#!/usr/bin/env dothttp

@name("Returns a list of employees")
GET "https://{{baseUrl}}/companies({{company_id}})/employees"
? "$top"= "<integer>"
? "$skip"= "<integer>"
? "$limit"= "<integer>"
? "$filter"= "<string>"
? "$expand"= "<string>,<string>"
? "$select"= "<string>,<string>"


