#!/usr/bin/env dothttp

@name("Returns a list of vendors")
GET "https://{{baseUrl}}/companies({{company_id}})/vendors"
? "$top"= "<integer>"
? "$skip"= "<integer>"
? "$limit"= "<integer>"
? "$filter"= "<string>"
? "$expand"= "<string>,<string>"
? "$select"= "<string>,<string>"


