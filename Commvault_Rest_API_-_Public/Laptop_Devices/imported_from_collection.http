#!/usr/bin/env dothttp

// <h2>Description</h2>
// This operation returns a list of laptop devices.

@name("Device")
GET "https://{{ServerUrl}}/Device"
"Accept": "{{Accept}}"
"Authtoken": "{{TOKEN}}"



// <h2>Description</h2>
// This operation returns the properties associated with a laptop device.

@name("Device Properties")
GET "https://{{ServerUrl}}/Device/{{deviceId}}"
"Accept": "{{Accept}}"
"Authtoken": "{{TOKEN}}"




