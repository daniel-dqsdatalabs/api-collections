#!/usr/bin/env dothttp

@name("Adding Disk Storage")
POST "https://{{ServerUrl}}/StoragePool"
"Authtoken": "{{TOKEN}}"
"Content-Type": "application/json"
"Accept": "{{Accept}}"
? "Action"= "create"
json({
    "storagePolicyName": "{{DiskstoragePolicyName}}",
    "type": 1,
    "copyName": "{{DiskcopyName}}",
    "numberOfCopies": 1,
    "clientGroup": {
        "_type_": 28,
        "clientGroupId": 0,
        "clientGroupName": ""
    },
    "storage": [
        {
            "path": "{{DiskstoragePath}}",
            "mediaAgent": {
                "_type_": 11,
                "mediaAgentName": "{{DiskmediaAgentName}}"
            },
            "credentials": {},
