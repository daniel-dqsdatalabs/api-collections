#!/usr/bin/env dothttp

@name("CreateIdentity")
POST "https://localhost/VTassure_00/VBS/REST/CreateIdentity"
basicauth("service", "Voice4All$")


@name("IsEnrolled")
GET "https://10.25.1.70/VTassure_00/VBS/REST/IsEnrolled"
basicauth("service", "Voice4All$")
? "identity"= "fa551fe0-0000-ff0f-0000-000000000000"
? "enrollmentConfiguration"= "fa551fe0-0000-ff0f-0000-000000000000"


@name("GetActiveCallList")
POST "https://10.25.1.70/VTassure_00/VBS/REST/GetActiveCallList"
basicauth("service", "Voice4All$")
? "verificationConfiguration"= "fa551fe0-0000-cf99-0000-000000000000"


@name("GetStateFromStream")
GET "https://10.25.1.70/VTassure_00/VBS/REST/GetStateFromStream"
basicauth("service", "Voice4All$")
? "SessionId"= ""


@name("EnrollFromStream")
POST "https://localhost/VTassure_00/VBS/REST/EnrollFromStream"
