@name("AdaptFromStream")
POST "https://10.25.1.70/VTassure_00/VBS/REST/AdaptFromStream"
basicauth("service", "Voice4All$")
? "SessionId"= ""
? "identity"= "bfd3190c-d3bd-45b1-a76e-6c8019fb786e"
? "enrollmentTag"= "agentdesktop"


@name("ResetStream")
POST "https://localhost/VTassure_00/VBS/REST/ResetStream"
