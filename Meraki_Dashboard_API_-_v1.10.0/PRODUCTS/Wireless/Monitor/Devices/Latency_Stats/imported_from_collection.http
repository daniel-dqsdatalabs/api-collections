#!/usr/bin/env dothttp

// Aggregated latency info for this network, grouped by node

@name("Aggregated latency info for this network, grouped by node")
GET "https://{{baseUrl}}/networks/:networkId/wireless/devices/latencyStats"
"X-Cisco-Meraki-API-Key": ""
? "t0"= "{{t0}}"
? "t1"= "{{t1}}"
? "timespan"= "{{timespan}}"
? "band"= "{{band}}"
? "ssid"= "{{ssid}}"
? "vlan"= "{{vlan}}"
? "apTag"= "{{apTag}}"
? "fields"= "{{fields}}"



