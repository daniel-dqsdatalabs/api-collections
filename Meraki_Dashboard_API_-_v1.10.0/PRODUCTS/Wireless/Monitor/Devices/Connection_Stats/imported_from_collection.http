#!/usr/bin/env dothttp

// Aggregated connectivity info for this network, grouped by node

@name("Aggregated connectivity info for this network, grouped by node")
GET "https://{{baseUrl}}/networks/:networkId/wireless/devices/connectionStats"
"X-Cisco-Meraki-API-Key": ""
? "t0"= "{{t0}}"
? "t1"= "{{t1}}"
? "timespan"= "{{timespan}}"
? "band"= "{{band}}"
? "ssid"= "{{ssid}}"
? "vlan"= "{{vlan}}"
? "apTag"= "{{apTag}}"



