#!/usr/bin/env dothttp

// Return the cellular firewall rules for an MX network
@name("Return The Cellular Firewall Rules For An MX Network")
GET "https://{{baseUrl}}/networks/:networkId/appliance/firewall/cellularFirewallRules"
"X-Cisco-Meraki-API-Key": ""


// Update the cellular firewall rules of an MX network//  #### Body Parameters // **Parameter**|**Type**|**Description** //  :-------------: |:-------------: |:-------------: // rules| array| An ordered array of the firewall rules (not including the default rule)
@name("Update The Cellular Firewall Rules Of An MX Network")
PUT "https://{{baseUrl}}/networks/:networkId/appliance/firewall/cellularFirewallRules"
"X-Cisco-Meraki-API-Key": ""
"Content-Type": "application/json"
data('{
    "rules": [
        {
            "policy": "<string>",
            "protocol": "<string>",
            "srcCidr": "<string>",
            "destCidr": "<string>",
            "comment": "<string>",
            "srcPort": "<string>",
            "destPort": "<string>",
            "syslogEnabled": "<boolean>"
        },
        {
            "policy": "<string>",
            "protocol": "<string>",
            "srcCidr": "<string>",
            "destCidr": "<string>",
            "comment": "<string>",
            "srcPort": "<string>",
            "destPort": "<string>",
            "syslogEnabled": "<boolean>"
        }
    ]
}')



