#!/usr/bin/env dothttp

// Returns a list of upcoming events.// // Link to Meetup API documentation: [https://www.meetup.com/meetup_api/docs/find/upcoming_events/](https://www.meetup.com/meetup_api/docs/find/upcoming_events/)
@name("Find upcoming events related to Postman")
GET "https://{{rootUrl}}/find/upcoming_events"
? "key"= "{{meetupKey}}"
? "start_date_range"= "{{startDate}}"
? "end_date_range"= "{{endDate}}"
? "text"= "Postman"
? "radius"= "global"
? "topic_category"= "292"


// Link to Meetup API documentation: [https://secure.meetup.com/meetup_api/console/?path=/:urlname](https://secure.meetup.com/meetup_api/console/?path=/:urlname)
@name("Find meetup group details")
GET "https://{{rootUrl}}/:urlname"


// Post to slack channel using Incoming Webhook.// // Link to Slack Incoming Webhook documentation: [https://api.slack.com/incoming-webhooks](https://api.slack.com/incoming-webhooks)
@name("Post to slack")
POST "https://{{slackWebhook}}/"
data('{{slackMessage}}')



