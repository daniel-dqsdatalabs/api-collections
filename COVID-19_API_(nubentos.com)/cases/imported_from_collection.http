#!/usr/bin/env dothttp

// Summary of suspected and confirmed
@name("Total of active cases.")
GET "https://{{base_url}}/cases"
? "country"= "<string>"


// Summary of suspected
@name("Total of suspected cases.")
GET "https://{{base_url}}/cases/suspected"


// Summary of confirmed
@name("Total of confirmed cases.")
GET "https://{{base_url}}/cases/confirmed"
? "country"= "<string>"



