#!/usr/bin/env dothttp

// To get the Notes associated with a Price Book.
@name("Notes")
GET "https://{{api-domain}}/crm/v2.1/Price_Books/{{record_id}}/Notes"


// To get the Products associated with a Price Books.
@name("Products")
GET "https://{{api-domain}}/crm/v2.1/Price_Books/{{record_id}}/Products"


// To get the Attachments associated with a Price Book.
@name("Attachments")
GET "https://{{api-domain}}/crm/v2.1/Price_Books/{{record_id}}/Attachments"



