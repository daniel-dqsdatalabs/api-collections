#!/usr/bin/env dothttp

// To upload files to the Zoho File System.
@name("Upload Files to Zoho File System (ZFS)")
POST "https://{{api-domain}}/crm/v2.1/files"
files(
	("file", "None")
)


// To fetch a file using its encrypted ID.
@name("Get Files")
GET "https://{{api-domain}}/crm/v2.1/files"
? "id"= "2cceafa194dxxxxxxxx4a20ed2"



