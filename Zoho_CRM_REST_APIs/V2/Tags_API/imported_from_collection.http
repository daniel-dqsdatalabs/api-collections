#!/usr/bin/env dothttp

// To merge tags and put all the records under the two tags into a single tag.
@name("Merge Tags")
POST "https://{{api-domain}}/crm/v2/settings/tags/{{tag_id}}/actions/merge"
json({
    "tags": [
        {
            "conflict_id": "{{id of the tag which you want to merge}}"
        }
    ]
})


// To delete a tag from a module.
@name("Delete Tags")
DELETE "https://{{api-domain}}/crm/v2/settings/tags/{{tag_id}}"



