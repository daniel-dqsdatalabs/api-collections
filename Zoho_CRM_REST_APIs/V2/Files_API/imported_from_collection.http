#!/usr/bin/env dothttp

// To upload files to the Zoho File System.

@name("Upload Files to Zoho File System (ZFS)")
POST "https://{{api-domain}}/crm/v2/files"
files(
	("file", "None")
)


// To fetch a file using its encrypted ID.

@name("Get Files")
GET "https://{{api-domain}}/crm/v2/files"
? "id"= "{{file_id}}"



