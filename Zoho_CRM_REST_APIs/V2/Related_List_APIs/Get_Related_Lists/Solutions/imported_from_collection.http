#!/usr/bin/env dothttp

// To get the Notes associated with a Solution.
@name("Notes")
GET "https://{{api-domain}}/crm/v2/Solutions/{{record_id}}/Notes"


// To get the Attachments associated with a Solution.
@name("Attachments")
GET "https://{{api-domain}}/crm/v2/Solutions/{{record_id}}/Attachments"



