#!/usr/bin/env dothttp

// This request lets you search for a specific journey after populating all the necessary parameters.

@name("Tickets")
GET "https://pass.rzd.ru/timetable/public/"
? "layer_id"= "5827"
? "dir"= "0"
? "code0"= "{{code0}}"
? "code1"= "{{code1}}"
? "tfl"= "{{tfl}}"
? "checkSeats"= "{{checkSeats}}"
? "dt0"= "{{dt0}}"
? "dt1"= "{{dt1}}"
? "md"= "{{md}}"



