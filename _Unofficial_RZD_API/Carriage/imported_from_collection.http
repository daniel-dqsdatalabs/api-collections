#!/usr/bin/env dothttp

// This request allows querying for the train stations of a specific journey. The response includes valuable information like the distance between the stations, waiting times as well as arrival and departure times.

@name("Get details of carriage")
GET "https://pass.rzd.ru/timetable/public/"
? "layer_id"= "5764"
? "dir"= "0"
? "code0"= "{{code0}}"
? "code1"= "{{code1}}"
? "dt0"= "{{dt0}}"
? "time0"= "00:00"
? "tnum0"= "189–ê"



