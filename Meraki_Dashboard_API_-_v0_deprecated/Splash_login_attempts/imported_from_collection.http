#!/usr/bin/env dothttp

// List the splash login attempts for a network
@name("getNetworkSplashLoginAttempts")
GET "https://{{baseUrl}}/networks/:networkId/splashLoginAttempts"
"Accept": "*/*"
? "ssidNumber"= "{{ssidNumber}}"
? "loginIdentifier"= "{{loginIdentifier}}"
? "timespan"= "{{timespan}}"




