#!/usr/bin/env dothttp

// Returns the port forwarding rules for a single MG.
@name("getDeviceCellularGatewaySettingsPortForwardingRules")
GET "https://{{baseUrl}}/devices/:serial/cellularGateway/settings/portForwardingRules"
"Accept": "*/*"



// Updates the port forwarding rules for a single MG.//  #### Body Parameters // **Parameter**|**Type**|**Description** //  :-------------: |:-------------: |:-------------: // rules| array| An array of port forwarding params
@name("updateDeviceCellularGatewaySettingsPortForwardingRules")
PUT "https://{{baseUrl}}/devices/:serial/cellularGateway/settings/portForwardingRules"
"Accept": "*/*"
"Content-Type": "application/json"
data('{
  "rules": [
    {
      "lanIp": "172.31.128.5",
      "name": "test",
      "access": "any",
      "publicPort": "11-12",
      "localPort": "4",
      "uplink": "both",
      "protocol": "tcp"
    },
    {
      "lanIp": "172.31.128.5",
      "name": "test 2",
      "access": "restricted",
      "allowedIps": [
        "10.10.10.10",
        "10.10.10.11"
      ],
      "publicPort": "99",
      "localPort": "5",
      "uplink": "both",
      "protocol": "tcp"
    }
  ]
}')



