#!/usr/bin/env dothttp

@name("Best Seller List")
GET "https://{{baseUrl}}/lists.{{format}}"
? "list"= "<string>"
? "weeks-on-list"= "<integer>"
? "bestsellers-date"= "<string>"
? "date"= "<string>"
? "isbn"= "<string>"
? "published-date"= "<string>"
? "rank"= "<integer>"
? "rank-last-week"= "<integer>"
? "offset"= "<integer>"
? "sort-order"= "<string>"


@name("Reviews")
GET "https://{{baseUrl}}/reviews.{{format}}"
? "isbn"= "<string>"
? "title"= "<string>"
? "author"= "<string>"
? "api-key"= "<string>"



