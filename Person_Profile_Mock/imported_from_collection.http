#!/usr/bin/env dothttp

// Returns a paged listing of all Assigned Tasks for the Person represented by the given id.

@name("Return all Assigned Tasks for a Person")
GET "https://{{url}}/api/v1/persons/59758618-c49a-430f-8c3a-6b122319ab31/tasks"
"": ""
? "criteria"= "<string>"
? "limit"= "{{limit}}"
? "offset"= "{{offset}}"




