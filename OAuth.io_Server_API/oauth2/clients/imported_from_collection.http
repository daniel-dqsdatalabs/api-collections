#!/usr/bin/env dothttp

@name("Create a client")
POST "https://{{baseUrl}}/oauth2/clients"


@name("Retrieves a client by its client_id")
GET "https://{{baseUrl}}/oauth2/clients/:client_id"


@name("Regenerate the API key's client")
GET "https://{{baseUrl}}/oauth2/clients/keygen/:client_id"


@name("Retrieves all clients, and filters by user_id if given")
GET "https://{{baseUrl}}/oauth2/clients/all/:user_id"



