#!/usr/bin/env dothttp

@name("egressmatrixcell")
GET "https://{{base_url}}/ers/config/egressmatrixcell"
basicauth("{{rest_username}}", "{{rest_password}}")
"Content-Type": "application/json"
"Accept": "application/json"
? "size"= "100"
? "page"= "1"
? "filter"= "name.CONTAINS.irv"
? "sortasc"= "name"
? "sortdsc"= "name"


