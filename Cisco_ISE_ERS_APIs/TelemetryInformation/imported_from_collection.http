#!/usr/bin/env dothttp

@name("telemetryinfo")
GET "https://{{base_url}}/ers/config/telemetryinfo"
basicauth("{{rest_username}}", "{{rest_password}}")
"Content-Type": "application/json"
"Accept": "application/json"


@name("telemetryinfo/{id}")
GET "https://{{base_url}}/ers/config/telemetryinfo/{{id}}"
basicauth("{{rest_username}}", "{{rest_password}}")
"Content-Type": "application/json"
"Accept": "application/json"



