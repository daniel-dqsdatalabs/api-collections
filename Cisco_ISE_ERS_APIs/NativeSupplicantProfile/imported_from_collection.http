#!/usr/bin/env dothttp

@name("nspprofile")
GET "https://{{base_url}}/ers/config/nspprofile"
basicauth("{{rest_username}}", "{{rest_password}}")
"Content-Type": "application/json"
"Accept": "application/json"
? "size"= "100"
? "page"= "1"
? "filter"= "name.CONTAINS.irv"
? "sortasc"= "name"
? "sortdsc"= "name"


@name("nspprofile/{id}")
GET "https://{{base_url}}/ers/config/nspprofile/{{id}}"
basicauth("{{rest_username}}", "{{rest_password}}")
"Content-Type": "application/json"
"Accept": "application/json"


