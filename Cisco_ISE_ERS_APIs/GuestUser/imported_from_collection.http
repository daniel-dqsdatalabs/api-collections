#!/usr/bin/env dothttp

@name("guestuser (âš  requires sponsor)")
GET "https://{{base_url}}/ers/config/guestuser"
basicauth("{{guestuser_username}}", "{{guestuser_password}}")
"Content-Type": "application/json"
"Accept": "application/json"
? "size"= "100"
? "page"= "1"
? "filter"= "name.CONTAINS.irv"
? "sortasc"= "name"
? "sortdsc"= "name"


