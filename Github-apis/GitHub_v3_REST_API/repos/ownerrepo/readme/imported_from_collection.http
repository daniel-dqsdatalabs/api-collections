#!/usr/bin/env dothttp

# {{baseUrl=api.github.com}}

@name("Get a repository README")
GET "https://{{baseUrl}}/repos/{{owner}}/{{repo}}/readme"
? "ref"= "aliqua consectetur elit incididu"


@name("Get a repository README for a directory")
GET "https://{{baseUrl}}/repos/{{owner}}/{{repo}}/readme/{{dir}}/"
? "ref"= "aliqua consectetur elit incididu"



