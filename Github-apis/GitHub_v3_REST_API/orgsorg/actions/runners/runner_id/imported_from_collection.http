#!/usr/bin/env dothttp

# {{baseUrl=api.github.com}}

@name("Get a self-hosted runner for an organization")
GET "https://{{baseUrl}}/orgs/{{org}}/actions/runners/{{runner_id}}/"


@name("Delete a self-hosted runner from an organization")
DELETE "https://{{baseUrl}}/orgs/{{org}}/actions/runners/{{runner_id}}/"



