#!/usr/bin/env dothttp

# {{baseUrl=api.github.com}}

@name("List SAML SSO authorizations for an organization")
GET "https://{{baseUrl}}/orgs/{{org}}/credential-authorizations"


@name("Remove a SAML SSO authorization for an organization")
DELETE "https://{{baseUrl}}/orgs/{{org}}/credential-authorizations/{{credential_id}}/"



