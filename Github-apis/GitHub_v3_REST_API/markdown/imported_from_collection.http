#!/usr/bin/env dothttp

# {{baseUrl=https://api.github.com}}

@name("Render a Markdown document")
POST "{{baseUrl}}/markdown"
"Content-Type": "application/json"
json({
    "text": "in magna commodo",
    "mode": "markdown",
    "context": "laboris dolore"
})


@name("Render a Markdown document in raw mode")
POST "{{baseUrl}}/markdown/raw"
"Content-Type": "text/plain"
data('"aliqua consectetur elit incididu"')



