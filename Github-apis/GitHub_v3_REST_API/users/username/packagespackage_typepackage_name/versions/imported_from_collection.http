#!/usr/bin/env dothttp

# {{baseUrl=api.github.com}}

@name("Get all package versions for a package owned by a user")
GET "https://{{baseUrl}}/users/{{username}}/packages/{{package_type}}/{{package_name}}/versions"


@name("Get a package version for a user")
GET "https://{{baseUrl}}/users/{{username}}/packages/{{package_type}}/{{package_name}}/versions/{{package_version_id}}/"



