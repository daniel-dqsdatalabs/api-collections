#!/usr/bin/env dothttp

@name("Create a folder")
POST "https://{{base_url}}/folders"
json({
    "name": "Marketing Campaign"
})


@name("Update a folder")
PATCH "https://{{base_url}}/folders/{{folder_id}}"
json({
    "name": "Marketing II"
})


@name("Delete a folder")
DELETE "https://{{base_url}}/folders/{{folder_id}}"




