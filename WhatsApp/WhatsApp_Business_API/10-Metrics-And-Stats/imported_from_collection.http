#!/usr/bin/env dothttp

@name("Metrics")
GET "https://{{URL}}/metrics"
"Authorization": "Bearer {{AdminAuthToken}}"


@name("Get-App-Stats")
GET "https://{{URL}}/v1/stats/app"
"Authorization": "Bearer {{AdminAuthToken}}"


@name("Get-DB-Stats")
GET "https://{{URL}}/v1/stats/db"
"Authorization": "Bearer {{AdminAuthToken}}"



