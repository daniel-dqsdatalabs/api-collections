#!/usr/bin/env dothttp

@name("Upload-Media")
POST "https://{{URL}}/v1/media"
"Content-Type": "image/jpeg"
"Authorization": "Bearer {{AdminAuthToken}}"
"": ""



@name("Download-Media")
GET "https://{{URL}}/v1/media/{{Test-Media-Id}}"
"Content-Type": "application/json"
"Authorization": "Bearer {{AdminAuthToken}}"


@name("Delete-Media")
DELETE "https://{{URL}}/v1/media/{{Test-Media-Id}}"
"Content-Type": "application/json"
"Authorization": "Bearer {{AdminAuthToken}}"




