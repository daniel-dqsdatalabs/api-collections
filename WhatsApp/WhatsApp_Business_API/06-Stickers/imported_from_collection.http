@name("Delete-Third-Party-Stickerpack")
DELETE "https://{{URL}}/v1/stickerpacks/{{Test-3P-Stickerpack-Id}}"
"Content-Type": "application/json"
"Authorization": "Bearer {{AdminAuthToken}}"


