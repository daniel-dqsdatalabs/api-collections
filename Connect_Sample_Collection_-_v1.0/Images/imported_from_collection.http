#!/usr/bin/env dothttp

@name("List a company's available images")
GET "https://connect.creditsafe.com/v1/images/companies"
? "id"= "{connectId}"
? "newerThan"= "2016-01-01T00:00:00"
? "olderThan"= "2020-01-01T00:00:00"
? "id"= "BE001-X-828450670"


@name("Order a specific Company Image")
GET "https://connect.creditsafe.com/v1/images/CO-BE-bbb9df5a3cc7d2e237296a902873d37c2a3850f1e66eae95c3e26eed87f15888"
? "id"= "{connectId}"



