@name('Update Policy - For Ad Hoc User Elevation Policy only - "AdHocElevate" Policy type')
PUT "https://{{EPMBaseURL}}/EPM/API/Sets/{{SetID}}/Policy/{{PolicyID}}"
"Authorization": "basic {{epmToken}}"
"VFUser": "{{epmToken}}"
"Content-Type": "application/json"
data('{
    "Duration": 60,
    "PolicyType": 12,
    "Name": "New Policy",
    "Audit":true,
    "Justification": "User Request",
    "TerminateProcesses": true,
    "IncludeUsers": [
        {
            "UserName": "Win10\\UserName",
            "Sid":""
        }
    ],
    "IncludeComputersInSet": [
        {
            "Id": "0ff92ffa-5097-4e00-adec-dc96ac5bbdb1"
        }
    ],
    "IncludeComputerGroups":[
        {
            "Id": "5fb75c0d-03b4-49a9-ade2-edb1e4daeeb4"
        }
    ],
    "IncludeADComputerGroups": [
        {
             "GroupName":"Group\\Name",
             "Sid":""
        }
    ],
    "LocalGroups": [
        {
            "GroupName": "OS_Group\\Name"
        }
    ]
}')


