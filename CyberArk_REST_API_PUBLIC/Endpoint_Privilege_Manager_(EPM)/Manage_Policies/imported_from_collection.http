@name("Update Ransomware Mode")
PATCH "https://{{EPMBaseURL}}/EPM/API/Sets/{{SetID}}/Policies/Ransomware"
"Authorization": "basic {{epmToken}}"
"VFUser": "{{epmToken}}"
"Content-Type": "application/json"
? "Mode"= "Enable"


