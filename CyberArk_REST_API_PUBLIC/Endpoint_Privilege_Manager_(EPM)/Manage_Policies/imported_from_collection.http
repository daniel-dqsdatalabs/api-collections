@name("Get Policies")
GET "https://{{EPMBaseURL}}/EPM/API/Sets/{{SetID}}/Policies"
"Authorization": "basic {{epmToken}}"
"VFUser": "{{epmToken}}"
"Content-Type": "application/json"


