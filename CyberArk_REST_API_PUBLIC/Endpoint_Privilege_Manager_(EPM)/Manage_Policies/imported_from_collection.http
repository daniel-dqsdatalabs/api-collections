@name("Get Policy Details")
GET "https://{{EPMBaseURL}}/EPM/API/Sets/{{SetID}}/Policies/{{PolicyID}}"
"Authorization": "basic {{epmToken}}"
"VFUser": "{{epmToken}}"
"Content-Type": "application/json"


