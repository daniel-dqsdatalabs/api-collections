@name("EPM Authentication")
POST "https://{{EPMBaseURL}}/EPM/API/Auth/EPM/Logon"
"Content-Type": "application/json"
data('{
  "Username": "{{apiUsername}}",
  "Password": "{{apiPassword}}",
  "ApplicationID": "Irrelevant"
}')


