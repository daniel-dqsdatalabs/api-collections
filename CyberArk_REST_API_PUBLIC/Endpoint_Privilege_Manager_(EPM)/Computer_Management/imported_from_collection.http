@name("Get Computers")
GET "https://{{EPMBaseURL}}/EPM/API/Sets/{{SetID}}/Computers"
"Authorization": "basic {{epmToken}}"
"VFUser": "{{epmToken}}"
"Content-Type": "application/json"


