@name("Inbox Events in the Last Day")
GET "https://{{BaseURL}}/EPM/API/Sets/{{SetID}}/Events/ApplicationEvents"
"Authorization": "basic {{epmToken}}"
"VFUser": "{{epmToken}}"
"Content-Type": "application/json"
? "EventType"= "ElevationRequest,Launch"
? "Publisher"= "Piriform Ltd"
? "Justification"= "WithJustification"
? "ApplicationType"= "Executable,Script,MSI,MSU,ActiveX,Com,Win8App,DLL"


