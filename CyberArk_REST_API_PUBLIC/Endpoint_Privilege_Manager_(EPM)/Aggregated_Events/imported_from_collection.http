@name("Policy Audit From Date & Policy Name")
GET "https://{{EPMBaseURL}}/EPM/API/Sets/{{SetID}}/Events/PolicyAudit"
"Authorization": "basic {{epmToken}}"
"VFUser": "{{epmToken}}"
"Content-Type": "application/json"
? "Publisher"= "Piriform Ltd"
? "DateFrom"= "2019-03-17"
? "DateTo"= "2019-03-19"
? "Justification"= "WithJustification"
? "ApplicationType"= "Executable,Script,MSI,MSU,ActiveX,Com,Win8App,DLL"
? "PolicyName"= "Network"


