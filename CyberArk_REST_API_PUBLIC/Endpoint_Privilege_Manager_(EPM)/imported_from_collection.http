@name("Raw Events")
GET "https://{{EPMBaseURL}}/EPM/API/Sets/{{SetID}}/Events/ApplicationEvents"
"Authorization": "basic {{epmToken}}"
"VFUser": "{{epmToken}}"
"Content-Type": "application/json"
? "EventQualifier"= "-1017619164156729424"


