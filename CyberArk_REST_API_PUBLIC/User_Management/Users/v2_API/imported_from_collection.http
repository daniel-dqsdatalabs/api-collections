// This method deletes a specific user in the Vault.
@name("Delete User")
DELETE "https://{{BaseURL}}/PasswordVault/api/Users/{{UserID}}"
"Authorization": "{{CyberArkLogonResult}}"
"Content-Type": "application/json"


// This method activates an existing Vault user who was suspended after entering incorrect credentials multiple times.// // **Note:** This method activates a suspended user. It does not activate an inactive user.
@name("Activate User")
POST "https://{{BaseURL}}/PasswordVault/api/Users/{{UserID}}/Activate"
"Authorization": "{{CyberArkLogonResult}}"
"Content-Type": "application/json"



