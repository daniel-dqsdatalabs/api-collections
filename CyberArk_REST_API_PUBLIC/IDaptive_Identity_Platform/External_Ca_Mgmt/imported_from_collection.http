#!/usr/bin/env dothttp

@name("{{baseUrl}}/ExternalCaMgmt/AddCertAuthority")
POST "https://{{baseUrl}}/ExternalCaMgmt/AddCertAuthority"
"Content-Type": "application/json"
data('{
    "externalCa": "<object>"
}')


@name("{{baseUrl}}/ExternalCaMgmt/DownloadCertAuthority")
POST "https://{{baseUrl}}/ExternalCaMgmt/DownloadCertAuthority"
? "externalCaId"= "<string>"


@name("{{baseUrl}}/ExternalCaMgmt/GetCertAuthorities")
POST "https://{{baseUrl}}/ExternalCaMgmt/GetCertAuthorities"


@name("{{baseUrl}}/ExternalCaMgmt/RemoveCertAuthority")
POST "https://{{baseUrl}}/ExternalCaMgmt/RemoveCertAuthority"
? "externalCaId"= "<string>"


@name("{{baseUrl}}/ExternalCaMgmt/UpdateCertAuthority")
POST "https://{{baseUrl}}/ExternalCaMgmt/UpdateCertAuthority"
"Content-Type": "application/json"
? "externalCaId"= "<string>"
data('{
    "externalCa": "<object>"
}')



