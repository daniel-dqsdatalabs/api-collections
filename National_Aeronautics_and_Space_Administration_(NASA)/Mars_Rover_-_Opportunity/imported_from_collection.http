#!/usr/bin/env dothttp

@name("By Sol")
GET "https://api.nasa.gov/mars-photos/api/v1/rovers/opportunity/photos"
? "sol"= "1000"
? "api_key"= "{{api_key}}"
? "page"= "1"


@name("By Earth Date")
GET "https://api.nasa.gov/mars-photos/api/v1/rovers/opportunity/photos"
? "earth_date"= "2015-6-3"
? "api_key"= "{{api_key}}"



