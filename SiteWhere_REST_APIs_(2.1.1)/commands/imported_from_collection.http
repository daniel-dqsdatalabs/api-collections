#!/usr/bin/env dothttp

@name("List device commands that match criteria.")
GET "https://{{baseUrl}}/commands"
"Authorization": ""
? "deviceTypeToken"= "<string>"
? "page"= "1"
? "pageSize"= "100"


@name("List device commands by namespace")
GET "https://{{baseUrl}}/commands/namespaces"
"Authorization": ""
? "deviceTypeToken"= "false"



