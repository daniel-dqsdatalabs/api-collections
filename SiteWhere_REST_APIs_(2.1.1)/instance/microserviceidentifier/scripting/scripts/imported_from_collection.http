#!/usr/bin/env dothttp

@name("Get list of global script metadata")
GET "https://{{baseUrl}}/instance/microservice/:identifier/scripting/scripts"
"Authorization": ""


@name("Create a new global script")
POST "https://{{baseUrl}}/instance/microservice/:identifier/scripting/scripts"
"Authorization": ""
"Content-Type": "application/json"
data('{
    "content": "<string>",
    "description": "<string>",
    "id": "<string>",
    "name": "<string>",
    "type": "<string>"
}')



