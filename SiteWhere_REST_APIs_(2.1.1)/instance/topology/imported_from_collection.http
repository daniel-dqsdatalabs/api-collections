#!/usr/bin/env dothttp

@name("Get current instance topology")
GET "https://{{baseUrl}}/instance/topology"
"Authorization": ""


@name("Get global microservices in current instance topology")
GET "https://{{baseUrl}}/instance/topology/global"
"Authorization": ""


@name("Get tenant microservices in current instance topology")
GET "https://{{baseUrl}}/instance/topology/tenant"
"Authorization": ""



