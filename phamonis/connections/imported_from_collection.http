#!/usr/bin/env dothttp

@name("decline connection")
POST "https://192.168.8.100/api/connections/decline-connection/:token"
files(
	("connectionId", "")
)


@name("fetch connections")
GET "https://192.168.8.100/api/connections/connections-list/:token"


@name("fetch connections staff")
GET "https://192.168.8.100/api/connections/connections-list-staff/:token"


@name("add request")
POST "https://192.168.8.100/api/connections/add-connection/:token"
files(
	("code", "282540"),
	("connection", "5dc83f703a2746316e875f08")
)


@name("fetch connection string")
GET "https://192.168.8.100/api/connections/connections-string/:token"



@name("accept request")
POST "https://192.168.8.100/api/connections/accept-connection/:token"
files(
	("connectionId", "")
)



