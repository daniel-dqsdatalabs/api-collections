#!/usr/bin/env dothttp

@name("Add device")
POST "https://localhost/api/pi/create-device/:token"



@name("Update Device")
POST "https://localhost/api/pi/update-device/:token/:device"



@name("Get device detail")
GET "https://localhost/api/pi/get-device-detail/:token/:device"


@name("Get All devices")
GET "https://localhost/api/pi/get-device-all/:token"


@name("Remove Device")
GET "https://localhost/api/pi/remove-device/:token/:device"



