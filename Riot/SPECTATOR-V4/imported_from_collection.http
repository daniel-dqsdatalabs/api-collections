#!/usr/bin/env dothttp

// Get current game information for the given summoner ID.
@name("Current Game by Summoner ID")
GET "https://{{URL}}/lol/spectator/v4/active-games/by-summoner/:Encrypted Summoner ID"


// Get list of featured games.
@name("Featured Games")
GET "https://{{URL}}/lol/spectator/v4/featured-games"



