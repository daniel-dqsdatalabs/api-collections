#!/usr/bin/env dothttp

@name("Returns all available resources for the provided vehicle identification number.")
GET "https://{{baseUrl}}/vehicles/:vehicleId/resources"


@name("Returns the latest available resource for the decklid status and vehicle identification number.")
GET "https://{{baseUrl}}/vehicles/:vehicleId/resources/decklidstatus"


@name("Returns the latest available resource for the front left door status and vehicle identification number.")
GET "https://{{baseUrl}}/vehicles/:vehicleId/resources/doorstatusfrontleft"


@name("Returns the latest available resource for the front right door status and vehicle identification number.")
GET "https://{{baseUrl}}/vehicles/:vehicleId/resources/doorstatusfrontright"


@name("Returns the latest available resource for the rear left door status and vehicle identification number.")
GET "https://{{baseUrl}}/vehicles/:vehicleId/resources/doorstatusrearleft"


@name("Returns the latest available resource for the rear right door status and vehicle identification number.")
GET "https://{{baseUrl}}/vehicles/:vehicleId/resources/doorstatusrearright"


@name("Returns the latest available resource for the interior front lights and vehicle identification number.")
