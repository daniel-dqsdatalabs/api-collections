#!/usr/bin/env dothttp

@name("Summary API")
GET "https://api-labs.symbl.ai/v1/conversations/{{conversation_id}}/summary"
"x-api-key": "{{api_token}}"
"Content-Type": "application/json"
? "refresh"= "true"


@name("Comprehensive Action Items")
GET "https://api-labs.symbl.ai/v1/conversations/{{conversation_id}}/comprehensive/action-items"
"x-api-key": "{{api_token}}"
"Content-Type": "application/json"



