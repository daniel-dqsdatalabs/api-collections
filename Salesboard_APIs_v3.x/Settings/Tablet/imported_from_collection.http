#!/usr/bin/env dothttp

@name("Get Tablet Settings")
GET "https://{{sb_client}}.salesboard.biz/v1/settings/tablet-settings"
"XREQUESTEDWITH": "WebAppHttpRequest"
"Cookie": "PHPSESSID={{sb_token}}"


@name("Get Tablet Settings for Team Id - not in use")
GET "https://{{sb_client}}.salesboard.biz/v1/settings/tablet-settings"
"XREQUESTEDWITH": "WebAppHttpRequest"
"Cookie": "PHPSESSID={{sb_token}}"
? "ID_Team"= "{{sb_team_id}}"



