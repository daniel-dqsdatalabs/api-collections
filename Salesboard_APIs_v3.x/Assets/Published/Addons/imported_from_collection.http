#!/usr/bin/env dothttp

@name("Get  Addon")
GET "https://{{sb_client}}.salesboard.biz/v1/assets/published/addons/{{sb_addon_id}}"
"XREQUESTEDWITH": "WebAppHttpRequest"
"cookie": "PHPSESSID={{sb_token}}"


@name("Get  Addon Resources")
GET "https://{{sb_client}}.salesboard.biz/v1/assets/published/addons/{{sb_addon_id}}/addon.zip"
"XREQUESTEDWITH": "WebAppHttpRequest"
"cookie": "PHPSESSID={{sb_token}}"



