#!/usr/bin/env dothttp

@name("Total Custody by Asset")
GET "https://{{API_BASE_URL}}/accounting/custody/asset/:assetId"


// Displays the total volume of the specified asset based on the chosen wallet.

@name("Total Custody by Wallet")
GET "https://{{API_BASE_URL}}/accounting/wallet/:walletId/asset/:assetId"


@name("Balance of wallets")
GET "https://{{API_BASE_URL}}/accounting/wallet-balance"
? "limit"= "25"
? "skip"= "0"



