#!/usr/bin/env dothttp

// Checks if the password or cvv of a given card is correct or not.

@name("/wallets/:walletId/cards/:cardId/check")
POST "https://{{API_BASE_URL}}/wallets/:walletId/cards/:id/check"
"Content-Type": "application/json"
data('{
	"asset": "BRLD",
	"type": "password",
	"value": "1235"
}')



